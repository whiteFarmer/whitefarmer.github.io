[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Modified from https://github.com/next-theme/hexo-theme-next/blob/master/scripts/events/lib/injects.js[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mconst fs = require('fs');[m
[32m+[m[32mconst path = require('path');[m
[32m+[m[32mconst defaultExtname = '.ejs';[m
[32m+[m
[32m+[m[32m// Defining stylus types[m
[32m+[m[32mclass StylusInject {[m
[32m+[m
[32m+[m[32m  constructor(base_dir) {[m
[32m+[m[32m    this.base_dir = base_dir;[m
[32m+[m[32m    this.files = [];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  push(file) {[m
[32m+[m[32m    // Get absolute path base on hexo dir[m
[32m+[m[32m    this.files.push(path.resolve(this.base_dir, file));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Defining view types[m
[32m+[m[32mclass ViewInject {[m
[32m+[m
[32m+[m[32m  constructor(base_dir) {[m
[32m+[m[32m    this.base_dir = base_dir;[m
[32m+[m[32m    this.raws = [];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  raw(name, raw, ...args) {[m
[32m+[m[32m    // Set default extname[m
[32m+[m[32m    if (path.extname(name) === '') {[m
[32m+[m[32m      name += defaultExtname;[m
[32m+[m[32m    }[m
[32m+[m[32m    this.raws.push({ name, raw, args });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  file(name, file, ...args) {[m
[32m+[m[32m    // Set default extname from file's extname[m
[32m+[m[32m    if (path.extname(name) === '') {[m
[32m+[m[32m      name += path.extname(file);[m
[32m+[m[32m    }[m
[32m+[m[32m    // Get absolute path base on hexo dir[m
[32m+[m[32m    this.raw(name, fs.readFileSync(path.resolve(this.base_dir, file), 'utf8'), ...args);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mconst points = {[m
[32m+[m[32m  views: [[m
[32m+[m[32m    'head',[m
[32m+[m[32m    'header',[m
[32m+[m[32m    'bodyBegin',[m
[32m+[m[32m    'bodyEnd',[m
[32m+[m[32m    'footer',[m
[32m+[m[32m    'postMetaTop',[m
[32m+[m[32m    'postMetaBottom',[m
[32m+[m[32m    'postMarkdownBegin',[m
[32m+[m[32m    'postMarkdownEnd',[m
[32m+[m[32m    'postLeft',[m
[32m+[m[32m    'postRight',[m
[32m+[m[32m    'postCopyright',[m
[32m+[m[32m    'postComments',[m
[32m+[m[32m    'pageComments',[m
[32m+[m[32m    'linksComments'[m
[32m+[m[32m  ],[m
[32m+[m[32m  styles: [[m
[32m+[m[32m    'variable',[m
[32m+[m[32m    'mixin',[m
[32m+[m[32m    'style'[m
[32m+[m[32m  ][m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Init injects[m
[32m+[m[32mfunction initInject(base_dir) {[m
[32m+[m[32m  const injects = {};[m
[32m+[m[32m  points.styles.forEach(item => {[m
[32m+[m[32m    injects[item] = new StylusInject(base_dir);[m
[32m+[m[32m  });[m
[32m+[m[32m  points.views.forEach(item => {[m
[32m+[m[32m    injects[item] = new ViewInject(base_dir);[m
[32m+[m[32m  });[m
[32m+[m[32m  return injects;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mmodule.exports = (hexo) => {[m
[32m+[m[32m  // Exec theme_inject filter[m
[32m+[m[32m  const injects = initInject(hexo.base_dir);[m
[32m+[m[32m  hexo.execFilterSync('theme_inject', injects);[m
[32m+[m[32m  hexo.theme.config.injects = {};[m
[32m+[m
[32m+[m[32m  // Inject stylus[m
[32m+[m[32m  points.styles.forEach(type => {[m
[32m+[m[32m    hexo.theme.config.injects[type] = injects[type].files;[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  // Inject views[m
[32m+[m[32m  points.views.forEach(type => {[m
[32m+[m[32m    const configs = Object.create(null);[m
[32m+[m[32m    hexo.theme.config.injects[type] = [];[m
[32m+[m[32m    // Add or override view.[m
[32m+[m[32m    injects[type].raws.forEach((injectObj, index) => {[m
[32m+[m[32m      const name = `inject/${type}/${injectObj.name}`;[m
[32m+[m[32m      hexo.theme.setView(name, injectObj.raw);[m
[32m+[m[32m      configs[name] = {[m
[32m+[m[32m        layout : name,[m
[32m+[m[32m        locals : injectObj.args[0],[m
[32m+[m[32m        options: injectObj.args[1],[m
[32m+[m[32m        order  : injectObj.args[2] || index[m
[32m+[m[32m      };[m
[32m+[m[32m    });[m
[32m+[m[32m    // Views sort.[m
[32m+[m[32m    hexo.theme.config.injects[type] = Object.values(configs)[m
[32m+[m[32m      .sort((x, y) => x.order - y.order);[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[1mdiff --git a/themes/fluid/scripts/events/lib/lazyload.js b/themes/fluid/scripts/events/lib/lazyload.js[m
[1mnew file mode 100644[m
[1mindex 0000000..6ee332a[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/scripts/events/lib/lazyload.js[m
[36m@@ -0,0 +1,50 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst urlJoin = require('../../utils/url-join');[m
[32m+[m
[32m+[m[32mmodule.exports = (hexo) => {[m
[32m+[m[32m  const config = hexo.theme.config;[m
[32m+[m[32m  const loadingImage = urlJoin(hexo.config.root, config.lazyload.loading_img[m
[32m+[m[32m    || urlJoin(config.static_prefix.internal_img, 'loading.gif'));[m
[32m+[m[32m  if (!config.lazyload || !config.lazyload.enable || !loadingImage) {[m
[32m+[m[32m    return;[m
[32m+[m[32m  }[m
[32m+[m[32m  if (config.lazyload.onlypost) {[m
[32m+[m[32m    hexo.extend.filter.register('after_post_render', (page) => {[m
[32m+[m[32m      if (page.layout !== 'post' && !page.lazyload) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m[32m      if (page.lazyload !== false) {[m
[32m+[m[32m        page.content = lazyImages(page.content, loadingImage);[m
[32m+[m[32m        page.content = lazyComments(page.content);[m
[32m+[m[32m      }[m
[32m+[m[32m      return page;[m
[32m+[m[32m    });[m
[32m+[m[32m  } else {[m
[32m+[m[32m    hexo.extend.filter.register('after_render:html', (html, data) => {[m
[32m+[m[32m      if (!data.page || data.page.lazyload !== false) {[m
[32m+[m[32m        html = lazyImages(html, loadingImage);[m
[32m+[m[32m        html = lazyComments(html);[m
[32m+[m[32m        return html;[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst lazyImages = (htmlContent, loadingImage) => {[m
[32m+[m[32m  return htmlContent.replace(/<img[^>]+?src=(".*?")[^>]*?>/gims, (str, p1) => {[m
[32m+[m[32m    if (/lazyload/i.test(str)) {[m
[32m+[m[32m      return str;[m
[32m+[m[32m    }[m
[32m+[m[32m    return str.replace(p1, `${p1} srcset="${loadingImage}" lazyload`);[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst lazyComments = (htmlContent) => {[m
[32m+[m[32m  return htmlContent.replace(/<[^>]+?id="comments"[^>]*?>/gims, (str) => {[m
[32m+[m[32m    if (/lazyload/i.test(str)) {[m
[32m+[m[32m      return str;[m
[32m+[m[32m    }[m
[32m+[m[32m    return str.replace('id="comments"', 'id="comments" lazyload');[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
