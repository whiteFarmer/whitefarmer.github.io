[1mdiff --git a/themes/fluid/scripts/events/lib/footnote.js b/themes/fluid/scripts/events/lib/footnote.js[m
[1mnew file mode 100644[m
[1mindex 0000000..b837a4b[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/scripts/events/lib/footnote.js[m
[36m@@ -0,0 +1,117 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst { stripHTML } = require('hexo-util');[m
[32m+[m
[32m+[m[32m// Register footnotes filter[m
[32m+[m[32mmodule.exports = (hexo) => {[m
[32m+[m[32m  const config = hexo.theme.config;[m
[32m+[m[32m  if (config.post.footnote.enable) {[m
[32m+[m[32m    hexo.extend.filter.register('before_post_render', (page) => {[m
[32m+[m[32m      if (page.footnote !== false) {[m
[32m+[m[32m        page.content = renderFootnotes(page.content, page.footnote);[m
[32m+[m[32m      }[m
[32m+[m[32m      return page;[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Modified from https://github.com/kchen0x/hexo-reference[m
[32m+[m[32m   *[m
[32m+[m[32m   * Render markdown footnotes[m
[32m+[m[32m   * @param {String} text[m
[32m+[m[32m   * @param {String} header[m
[32m+[m[32m   * @returns {String} text[m
[32m+[m[32m   */[m
[32m+[m[32m  function renderFootnotes(text, header) {[m
[32m+[m[32m    const reFootnoteContent = /\[\^(\d+)]: ?([\S\s]+?)(?=\[\^(?:\d+)]|\n\n|$)/g;[m
[32m+[m[32m    const reInlineFootnote = /\[\^(\d+)]\((.+?)\)/g;[m
[32m+[m[32m    const reFootnoteIndex = /\[\^(\d+)]/g;[m
[32m+[m[32m    const reCodeBlock = /<pre>[\s\S]*?<\/pre>/g;[m
[32m+[m
[32m+[m[32m    let footnotes = [];[m
[32m+[m[32m    let html = '';[m
[32m+[m[32m    let codeBlocks = [];[m
[32m+[m
[32m+[m[32m    // extract code block[m
[32m+[m[32m    text = text.replace(reCodeBlock, function(match) {[m
[32m+[m[32m      codeBlocks.push(match);[m
[32m+[m[32m      return 'CODE_BLOCK_PLACEHOLDER';[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // threat all inline footnotes[m
[32m+[m[32m    text = text.replace(reInlineFootnote, function(match, index, content) {[m
[32m+[m[32m      footnotes.push({[m
[32m+[m[32m        index  : index,[m
[32m+[m[32m        content: content ? content.trim() : ''[m
[32m+[m[32m      });[m
[32m+[m[32m      // remove content of inline footnote[m
[32m+[m[32m      return '[^' + index + ']';[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // threat all footnote contents[m
[32m+[m[32m    text = text.replace(reFootnoteContent, function(match, index, content) {[m
[32m+[m[32m      footnotes.push({[m
[32m+[m[32m        index  : index,[m
[32m+[m[32m        content: content ? content.trim() : ''[m
[32m+[m[32m      });[m
[32m+[m[32m      // remove footnote content[m
[32m+[m[32m      return '';[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // create map for looking footnotes array[m
[32m+[m[32m    function createLookMap(field) {[m
[32m+[m[32m      let map = {};[m
[32m+[m[32m      for (let i = 0; i < footnotes.length; i++) {[m
[32m+[m[32m        const item = footnotes[i];[m
[32m+[m[32m        const key = item[field];[m
[32m+[m[32m        map[key] = item;[m
[32m+[m[32m      }[m
[32m+[m[32m      return map;[m
[32m+[m[32m    }[m
[32m+[m[32m    const indexMap = createLookMap('index');[m
[32m+[m
[32m+[m[32m    // render (HTML) footnotes reference[m
[32m+[m[32m    text = text.replace(reFootnoteIndex,[m
[32m+[m[32m      function(match, index) {[m
[32m+[m[32m        if (!indexMap[index]) {[m
[32m+[m[32m          return match;[m
[32m+[m[32m        }[m
[32m+[m[32m        const tooltip = indexMap[index].content;[m
[32m+[m[32m        return '<sup id="fnref:' + index + '" class="footnote-ref">'[m
[32m+[m[32m          + '<a href="#fn:' + index + '" rel="footnote">'[m
[32m+[m[32m          + '<span class="hint--top hint--rounded" aria-label="'[m
[32m+[m[32m          + stripHTML(tooltip)[m
[32m+[m[32m          + '">[' + index + ']</span></a></sup>';[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m    // sort footnotes by their index[m
[32m+[m[32m    footnotes.sort(function(a, b) {[m
[32m+[m[32m      return a.index - b.index;[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // render footnotes (HTML)[m
[32m+[m[32m    footnotes.forEach(function(item) {[m
[32m+[m[32m      html += '<li><span id="fn:' + item.index + '" class="footnote-text">';[m
[32m+[m[32m      html += '<span>';[m
[32m+[m[32m      const fn = hexo.render.renderSync({ text: item.content, engine: 'markdown' });[m
[32m+[m[32m      html += fn.replace(/(<p>)|(<\/p>)/g, '').replace(/<br>/g, '');[m
[32m+[m[32m      html += '<a href="#fnref:' + item.index + '" rev="footnote" class="footnote-backref"> â†©</a></span></span></li>';[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // add footnotes at the end of the content[m
[32m+[m[32m    if (footnotes.length) {[m
[32m+[m[32m      text += '<section class="footnotes">';[m
[32m+[m[32m      text += header || config.post.footnote.header || '';[m
[32m+[m[32m      text += '<div class="footnote-list">';[m
[32m+[m[32m      text += '<ol>' + html + '</ol>';[m
[32m+[m[32m      text += '</div></section>';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // restore code block[m
[32m+[m[32m    text = text.replace(/CODE_BLOCK_PLACEHOLDER/g, function() {[m
[32m+[m[32m      return codeBlocks.shift();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    return text;[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
