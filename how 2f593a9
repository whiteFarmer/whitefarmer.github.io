[32m+[m[32m/* global hexo */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst checkbox = (args) => {[m
[32m+[m[32m  args = args[0] === ',' ? args.slice(1) : args;[m
[32m+[m[32m  args = args.join(' ').split(',');[m
[32m+[m[32m  const text = (args[0] || '').trim();[m
[32m+[m
[32m+[m[32m  !text && hexo.log.warn('[Fluid] Checkbox text must be defined!');[m
[32m+[m
[32m+[m[32m  if (text === 'checked' || text === 'true' || text === 'false') {[m
