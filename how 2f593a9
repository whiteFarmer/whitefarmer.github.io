[1m+++ b/themes/fluid/layout/_partials/category-list.ejs[m
[36m@@ -0,0 +1,62 @@[m
[32m+[m[32m<% function render_categories(curCats, params = {}, depth = 0) { %>[m
[32m+[m[32m  <% return curCats.each((cat) => { %>[m
[32m+[m[32m    <% var subCats = site.categories.find({ parent: cat._id }).sort(params.orderBy || 'name').filter(cat => cat.length) %>[m
[32m+[m[32m    <% var collapsed = subCats.length === 0 || depth >= theme.category.collapse_depth %>[m
[32m+[m[32m    <% if ((params.filterIds || []).includes(cat._id)) collapsed = false %>[m
[32m+[m[32m    <div class="<%= depth <= 0 ? 'category' : 'category-sub' %> row nomargin-x">[m
[32m+[m[32m      <a class="<%= depth <= 0 ? 'category-item' : 'category-subitem' %> <%= collapsed ? 'collapsed' : '' %>[m
[32m+[m[32m          list-group-item category-item-action col-10 col-md-11 col-xm-11" title="<%= cat.name.trim() %>"[m
[32m+[m[32m        id="heading-<%= md5(cat.name) %>" role="tab" data-toggle="collapse" href="#collapse-<%= md5(cat.name) %>"[m
[32m+[m[32m        aria-expanded="<%= collapsed ? 'false' : 'true' %>"[m
[32m+[m[32m      >[m
[32m+[m[32m        <%= cat.name.trim() %>[m
[32m+[m[32m        <span class="list-group-count"><%= params.type === 'post' ? `(${ cat.posts.length })` : '' %></span>[m
[32m+[m[32m        <i class="iconfont icon-arrowright"></i>[m
[32m+[m[32m      </a>[m
[32m+[m[32m      <% if(params.type !== 'post') { %>[m
[32m+[m[32m        <a href="<%= url_for(cat.path) %>" class="category-count col-2 col-md-1 col-xm-1">[m
[32m+[m[32m          <i class="iconfont icon-articles"></i>[m
[32m+[m[32m          <span><%= cat.posts.length %></span>[m
[32m+[m[32m        </a>[m
[32m+[m[32m      <% } %>[m
[32m+[m[32m      <div class="category-collapse collapse <%= collapsed ? '' : 'show' %>" id="collapse-<%= md5(cat.name) %>"[m
[32m+[m[32m           role="tabpanel" aria-labelledby="heading-<%= md5(cat.name) %>">[m
[32m+[m[32m        <% var posts = cat.posts.sort(params.postOrderBy || '-date') %>[m
[32m+[m[32m        <% if (subCats.length > 0) { %>[m
[32m+[m[32m          <% var filteredPosts = posts.filter((p) => {[m
[32m+[m[32m            return p.categories.filter(catOnPost => catOnPost.parent === cat._id).length === 0;[m
[32m+[m[32m          }) %>[m
[32m+[m[32m          <%- render_posts(filteredPosts, cat, params) %>[m
[32m+[m[32m          <%- render_categories(subCats, params, depth + 1) %>[m
[32m+[m[32m        <% } else { %>[m
[32m+[m[32m          <%- render_posts(posts, cat, params) %>[m
[32m+[m[32m        <% } %>[m
[32m+[m[32m      </div>[m
[32m+[m[32m    </div>[m
[32m+[m[32m  <% }) %>[m
[32m+[m[32m<% } %>[m
[32m+[m
[32m+[m[32m<% function render_posts(posts, cat, params) { %>[m
[32m+[m[32m  <div class="category-post-list">[m
[32m+[m[32m    <% var limit = params.postLimit %>[m
[32m+[m[32m    <% for (var idx = 0; idx < posts.length; idx++) { %>[m
[32m+[m[32m      <% var post = posts.data[idx] %>[m
[32m+[m[32m      <% if (idx && limit && idx >= limit) { %>[m
[32m+[m[32m        <a href="<%= url_for(cat.path) %>" class="list-group-item list-group-item-action">[m
[32m+[m[32m          <span class="category-post"><%- __('category.more') %></span>[m
[32m+[m[32m        </a>[m
[32m+[m[32m        <% break %>[m
[32m+[m[32m      <% } else { %>[m
[32m+[m[32m        <a href="<%= url_for(post.path) %>" title="<%= post.title %>"[m
[32m+[m[32m           class="list-group-item list-group-item-action[m
[32m+[m[32m           <%= (params.filterIds || []).includes(post._id) ? 'active' : ''  %>">[m
[32m+[m[32m          <span class="category-post"><%= post.title %></span>[m
[32m+[m[32m        </a>[m
[32m+[m[32m      <% } %>[m
[32m+[m[32m    <% } %>[m
[32m+[m[32m  </div>[m
[32m+[m[32m<% } %>[m
[32m+[m
[32m+[m[32m<div class="category-list">[m
[32m+[m[32m  <%- render_categories(curCats, params) %>[m
[32m+[m[32m</div>[m
