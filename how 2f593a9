[1mdiff --git a/themes/fluid/source/js/leancloud.js b/themes/fluid/source/js/leancloud.js[m
[1mnew file mode 100644[m
[1mindex 0000000..ab901ce[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/source/js/leancloud.js[m
[36m@@ -0,0 +1,192 @@[m
[32m+[m[32m/* global CONFIG */[m
[32m+[m[32m// eslint-disable-next-line no-console[m
[32m+[m
[32m+[m[32m(function(window, document) {[m
[32m+[m[32m  // æŸ¥è¯¢å­˜å‚¨çš„è®°å½•[m
[32m+[m[32m  function getRecord(Counter, target) {[m
[32m+[m[32m    return new Promise(function(resolve, reject) {[m
[32m+[m[32m      Counter('get', '/classes/Counter?where=' + encodeURIComponent(JSON.stringify({ target })))[m
[32m+[m[32m        .then(resp => resp.json())[m
[32m+[m[32m        .then(({ results, code, error }) => {[m
[32m+[m[32m          if (code === 401) {[m
[32m+[m[32m            throw error;[m
[32m+[m[32m          }[m
[32m+[m[32m          if (results && results.length > 0) {[m
[32m+[m[32m            var record = results[0];[m
[32m+[m[32m            resolve(record);[m
[32m+[m[32m          } else {[m
[32m+[m[32m            Counter('post', '/classes/Counter', { target, time: 0 })[m
[32m+[m[32m              .then(resp => resp.json())[m
[32m+[m[32m              .then((record, error) => {[m
[32m+[m[32m                if (error) {[m
[32m+[m[32m                  throw error;[m
[32m+[m[32m                }[m
[32m+[m[32m                resolve(record);[m
[32m+[m[32m              }).catch(error => {[m
[32m+[m[32m                console.error('Failed to create: ', error);[m
[32m+[m[32m                reject(error);[m
[32m+[m[32m              });[m
[32m+[m[32m          }[m
[32m+[m[32m        }).catch((error) => {[m
[32m+[m[32m          console.error('LeanCloud Counter Error: ', error);[m
[32m+[m[32m          reject(error);[m
[32m+[m[32m        });[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // å‘èµ·è‡ªå¢žè¯·æ±‚[m
[32m+[m[32m  function increment(Counter, incrArr) {[m
[32m+[m[32m    return new Promise(function(resolve, reject) {[m
[32m+[m[32m      Counter('post', '/batch', {[m
[32m+[m[32m        'requests': incrArr[m
[32m+[m[32m      }).then((res) => {[m
[32m+[m[32m        res = res.json();[m
[32m+[m[32m        if (res.error) {[m
[32m+[m[32m          throw res.error;[m
[32m+[m[32m        }[m
[32m+[m[32m        resolve(res);[m
[32m+[m[32m      }).catch((error) => {[m
[32m+[m[32m        console.error('Failed to save visitor count: ', error);[m
[32m+[m[32m        reject(error);[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // æž„å»ºè‡ªå¢žè¯·æ±‚ä½“[m
[32m+[m[32m  function buildIncrement(objectId) {[m
[32m+[m[32m    return {[m
[32m+[m[32m      'method': 'PUT',[m
[32m+[m[32m      'path'  : `/1.1/classes/Counter/${objectId}`,[m
[32m+[m[32m      'body'  : {[m
[32m+[m[32m        'time': {[m
[32m+[m[32m          '__op'  : 'Increment',[m
[32m+[m[32m          'amount': 1[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // æ ¡éªŒæ˜¯å¦ä¸ºæœ‰æ•ˆçš„ Host[m
[32m+[m[32m  function validHost() {[m
[32m+[m[32m    if (CONFIG.web_analytics.leancloud.ignore_local) {[m
[32m+[m[32m      var hostname = window.location.hostname;[m
[32m+[m[32m      if (hostname === 'localhost' || hostname === '127.0.0.1') {[m
[32m+[m[32m        return false;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    return true;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // æ ¡éªŒæ˜¯å¦ä¸ºæœ‰æ•ˆçš„ UV[m
[32m+[m[32m  function validUV() {[m
[32m+[m[32m    var key = 'LeanCloud_UV_Flag';[m
[32m+[m[32m    var flag = localStorage.getItem(key);[m
[32m+[m[32m    if (flag) {[m
[32m+[m[32m      // è·ç¦»æ ‡è®°å°äºŽ 24 å°æ—¶åˆ™ä¸è®¡ä¸º UV[m
[32m+[m[32m      if (new Date().getTime() - parseInt(flag, 10) <= 86400000) {[m
[32m+[m[32m        return false;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    localStorage.setItem(key, new Date().getTime().toString());[m
[32m+[m[32m    return true;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function addCount(Counter) {[m
[32m+[m[32m    var enableIncr = CONFIG.web_analytics.enable && !Fluid.ctx.dnt && validHost();[m
[32m+[m[32m    var getterArr = [];[m
[32m+[m[32m    var incrArr = [];[m
[32m+[m
[32m+[m[32m    // è¯·æ±‚ PV å¹¶è‡ªå¢ž[m
[32m+[m[32m    var pvCtn = document.querySelector('#leancloud-site-pv-container');[m
[32m+[m[32m    if (pvCtn) {[m
[32m+[m[32m      var pvGetter = getRecord(Counter, 'site-pv').then((record) => {[m
[32m+[m[32m        enableIncr && incrArr.push(buildIncrement(record.objectId));[m
[32m+[m[32m        var ele = document.querySelector('#leancloud-site-pv');[m
[32m+[m[32m        if (ele) {[m
[32m+[m[32m          ele.innerText = (record.time || 0) + (enableIncr ? 1 : 0);[m
[32m+[m[32m          pvCtn.style.display = 'inline';[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m      getterArr.push(pvGetter);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // è¯·æ±‚ UV å¹¶è‡ªå¢ž[m
[32m+[m[32m    var uvCtn = document.querySelector('#leancloud-site-uv-container');[m
[32m+[m[32m    if (uvCtn) {[m
[32m+[m[32m      var uvGetter = getRecord(Counter, 'site-uv').then((record) => {[m
[32m+[m[32m        var incrUV = validUV() && enableIncr;[m
[32m+[m[32m        incrUV && incrArr.push(buildIncrement(record.objectId));[m
[32m+[m[32m        var ele = document.querySelector('#leancloud-site-uv');[m
[32m+[m[32m        if (ele) {[m
[32m+[m[32m          ele.innerText = (record.time || 0) + (incrUV ? 1 : 0);[m
[32m+[m[32m          uvCtn.style.display = 'inline';[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m      getterArr.push(uvGetter);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // å¦‚æžœæœ‰é¡µé¢æµè§ˆæ•°èŠ‚ç‚¹ï¼Œåˆ™è¯·æ±‚æµè§ˆæ•°å¹¶è‡ªå¢ž[m
[32m+[m[32m    var viewCtn = document.querySelector('#leancloud-page-views-container');[m
[32m+[m[32m    if (viewCtn) {[m
[32m+[m[32m      var path = eval(CONFIG.web_analytics.leancloud.path || 'window.location.pathname');[m
[32m+[m[32m      var target = decodeURI(path.replace(/\/*(index.html)?$/, '/'));[m
[32m+[m[32m      var viewGetter = getRecord(Counter, target).then((record) => {[m
[32m+[m[32m        enableIncr && incrArr.push(buildIncrement(record.objectId));[m
[32m+[m[32m        var ele = document.querySelector('#leancloud-page-views');[m
[32m+[m[32m        if (ele) {[m
[32m+[m[32m          ele.innerText = (record.time || 0) + (enableIncr ? 1 : 0);[m
[32m+[m[32m          viewCtn.style.display = 'inline';[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m      getterArr.push(viewGetter);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // å¦‚æžœå¯åŠ¨è®¡æ•°è‡ªå¢žï¼Œæ‰¹é‡å‘èµ·è‡ªå¢žè¯·æ±‚[m
[32m+[m[32m    if (enableIncr) {[m
[32m+[m[32m      Promise.all(getterArr).then(() => {[m
[32m+[m[32m        incrArr.length > 0 && increment(Counter, incrArr);[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var appId = CONFIG.web_analytics.leancloud.app_id;[m
[32m+[m[32m  var appKey = CONFIG.web_analytics.leancloud.app_key;[m
[32m+[m[32m  var serverUrl = CONFIG.web_analytics.leancloud.server_url;[m
[32m+[m
[32m+[m[32m  if (!appId) {[m
[32m+[m[32m    throw new Error('LeanCloud appId is empty');[m
[32m+[m[32m  }[m
[32m+[m[32m  if (!appKey) {[m
[32m+[m[32m    throw new Error('LeanCloud appKey is empty');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function fetchData(api_server) {[m
[32m+[m[32m    var Counter = (method, url, data) => {[m
[32m+[m[32m      return fetch(`${api_server}/1.1${url}`, {[m
[32m+[m[32m        method,[m
[32m+[m[32m        headers: {[m
[32m+[m[32m          'X-LC-Id'     : appId,[m
[32m+[m[32m          'X-LC-Key'    : appKey,[m
[32m+[m[32m          'Content-Type': 'application/json'[m
[32m+[m[32m        },[m
[32m+[m[32m        body: JSON.stringify(data)[m
[32m+[m[32m      });[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    addCount(Counter);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var apiServer = serverUrl || `https://${appId.slice(0, 8).toLowerCase()}.api.lncldglobal.com`;[m
[32m+[m
[32m+[m[32m  if (apiServer) {[m
[32m+[m[32m    fetchData(apiServer);[m
[32m+[m[32m  } else {[m
[32m+[m[32m    fetch('https://app-router.leancloud.cn/2/route?appId=' + appId)[m
[32m+[m[32m      .then(resp => resp.json())[m
[32m+[m[32m      .then((data) => {[m
[32m+[m[32m        if (data.api_server) {[m
[32m+[m[32m          fetchData('https://' + data.api_server);[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m  }[m
[32m+[m[32m})(window, document);[m
[1mdiff --git a/themes/fluid/source/js/local-search.js b/themes/fluid/source/js/local-search.js[m
[1mnew file mode 100644[m
[1mindex 0000000..0784a80[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/source/js/local-search.js[m
[36m@@ -0,0 +1,159 @@[m
[32m+[m[32m/* global CONFIG */[m
[32m+[m
[32m+[m[32m(function() {[m
[32m+[m[32m  // Modified from [hexo-generator-search](https://github.com/wzpan/hexo-generator-search)[m
[32m+[m[32m  function localSearchFunc(path, searchSelector, resultSelector) {[m
[32m+[m[32m    'use strict';[m
[32m+[m[32m    // 0x00. environment initialization[m
[32m+[m[32m    var $input = jQuery(searchSelector);[m
[32m+[m[32m    var $result = jQuery(resultSelector);[m
[32m+[m
[32m+[m[32m    if ($input.length === 0) {[m
[32m+[m[32m      // eslint-disable-next-line no-console[m
[32m+[m[32m      throw Error('No element selected by the searchSelector');[m
[32m+[m[32m    }[m
[32m+[m[32m    if ($result.length === 0) {[m
[32m+[m[32m      // eslint-disable-next-line no-console[m
[32m+[m[32m      throw Error('No element selected by the resultSelector');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if ($result.attr('class').indexOf('list-group-item') === -1) {[m
[32m+[m[32m      $result.html('<div class="m-auto text-center"><div class="spinner-border" role="status"><span class="sr-only">Loading...</span></div><br/>Loading...</div>');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    jQuery.ajax({[m
[32m+[m[32m      // 0x01. load xml file[m
[32m+[m[32m      url     : path,[m
[32m+[m[32m      dataType: 'xml',[m
[32m+[m[32m      success : function(xmlResponse) {[m
[32m+[m[32m        // 0x02. parse xml file[m
[32m+[m[32m        var dataList = jQuery('entry', xmlResponse).map(function() {[m
[32m+[m[32m          return {[m
[32m+[m[32m            title  : jQuery('title', this).text(),[m
[32m+[m[32m            content: jQuery('content', this).text(),[m
[32m+[m[32m            url    : jQuery('url', this).text()[m
[32m+[m[32m          };[m
[32m+[m[32m        }).get();[m
[32m+[m
[32m+[m[32m        if ($result.html().indexOf('list-group-item') === -1) {[m
[32m+[m[32m          $result.html('');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $input.on('input', function() {[m
[32m+[m[32m          // 0x03. parse query to keywords list[m
[32m+[m[32m          var content = $input.val();[m
[32m+[m[32m          var resultHTML = '';[m
[32m+[m[32m          var keywords = content.trim().toLowerCase().split(/[\s-]+/);[m
[32m+[m[32m          $result.html('');[m
[32m+[m[32m          if (content.trim().length <= 0) {[m
[32m+[m[32m            return $input.removeClass('invalid').removeClass('valid');[m
[32m+[m[32m          }[m
[32m+[m[32m          // 0x04. perform local searching[m
[32m+[m[32m          dataList.forEach(function(data) {[m
[32m+[m[32m            var isMatch = true;[m
[32m+[m[32m            if (!data.title || data.title.trim() === '') {[m
[32m+[m[32m              data.title = 'Untitled';[m
[32m+[m[32m            }[m
[32m+[m[32m            var orig_data_title = data.title.trim();[m
[32m+[m[32m            var data_title = orig_data_title.toLowerCase();[m
[32m+[m[32m            var orig_data_content = data.content.trim().replace(/<[^>]+>/g, '');[m
[32m+[m[32m            var data_content = orig_data_content.toLowerCase();[m
[32m+[m[32m            var data_url = data.url;[m
[32m+[m[32m            var index_title = -1;[m
[32m+[m[32m            var index_content = -1;[m
[32m+[m[32m            var first_occur = -1;[m
[32m+[m[32m            // Skip matching when content is included in search and content is empty[m
[32m+[m[32m            if (CONFIG.include_content_in_search && data_content === '') {[m
[32m+[m[32m              isMatch = false;[m
[32m+[m[32m            } else {[m
[32m+[m[32m              keywords.forEach(function (keyword, i) {[m
[32m+[m[32m                index_title = data_title.indexOf(keyword);[m
[32m+[m[32m                index_content = data_content.indexOf(keyword);[m
[32m+[m
[32m+[m[32m                if (index_title < 0 && index_content < 0) {[m
[32m+[m[32m                  isMatch = false;[m
[32m+[m[32m                } else {[m
[32m+[m[32m                  if (index_content < 0) {[m
[32m+[m[32m                    index_content = 0;[m
[32m+[m[32m                  }[m
[32m+[m[32m                  if (i === 0) {[m
[32m+[m[32m                    first_occur = index_content;[m
[32m+[m[32m                  }[m
[32m+[m[32m                }[m
[32m+[m[32m              });[m
[32m+[m[32m            }[m
[32m+[m[32m            // 0x05. show search results[m
[32m+[m[32m            if (isMatch) {[m
[32m+[m[32m              resultHTML += '<a href=\'' + data_url + '\' class=\'list-group-item list-group-item-action font-weight-bolder search-list-title\'>' + orig_data_title + '</a>';[m
[32m+[m[32m              var content = orig_data_content;[m
[32m+[m[32m              if (first_occur >= 0) {[m
[32m+[m[32m                // cut out 100 characters[m
[32m+[m[32m                var start = first_occur - 20;[m
[32m+[m[32m                var end = first_occur + 80;[m
[32m+[m
[32m+[m[32m                if (start < 0) {[m
[32m+[m[32m                  start = 0;[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                if (start === 0) {[m
[32m+[m[32m                  end = 100;[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                if (end > content.length) {[m
[32m+[m[32m                  end = content.length;[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                var match_content = content.substring(start, end);[m
[32m+[m
[32m+[m[32m                // highlight all keywords[m
[32m+[m[32m                keywords.forEach(function(keyword) {[m
[32m+[m[32m                  var regS = new RegExp(keyword, 'gi');[m
[32m+[m[32m                  match_content = match_content.replace(regS, '<span class="search-word">' + keyword + '</span>');[m
[32m+[m[32m                });[m
[32m+[m
[32m+[m[32m                resultHTML += '<p class=\'search-list-content\'>' + match_content + '...</p>';[m
[32m+[m[32m              }[m
[32m+[m[32m            }[m
[32m+[m[32m          });[m
[32m+[m[32m          if (resultHTML.indexOf('list-group-item') === -1) {[m
[32m+[m[32m            return $input.addClass('invalid').removeClass('valid');[m
[32m+[m[32m          }[m
[32m+[m[32m          $input.addClass('valid').removeClass('invalid');[m
[32m+[m[32m          $result.html(resultHTML);[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function localSearchReset(searchSelector, resultSelector) {[m
[32m+[m[32m    'use strict';[m
[32m+[m[32m    var $input = jQuery(searchSelector);[m
[32m+[m[32m    var $result = jQuery(resultSelector);[m
[32m+[m
[32m+[m[32m    if ($input.length === 0) {[m
[32m+[m[32m      // eslint-disable-next-line no-console[m
[32m+[m[32m      throw Error('No element selected by the searchSelector');[m
[32m+[m[32m    }[m
[32m+[m[32m    if ($result.length === 0) {[m
[32m+[m[32m      // eslint-disable-next-line no-console[m
[32m+[m[32m      throw Error('No element selected by the resultSelector');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $input.val('').removeClass('invalid').removeClass('valid');[m
[32m+[m[32m    $result.html('');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var modal = jQuery('#modalSearch');[m
[32m+[m[32m  var searchSelector = '#local-search-input';[m
[32m+[m[32m  var resultSelector = '#local-search-result';[m
[32m+[m[32m  modal.on('show.bs.modal', function() {[m
[32m+[m[32m    var path = CONFIG.search_path || '/local-search.xml';[m
[32m+[m[32m    localSearchFunc(path, searchSelector, resultSelector);[m
[32m+[m[32m  });[m
[32m+[m[32m  modal.on('shown.bs.modal', function() {[m
[32m+[m[32m    jQuery('#local-search-input').focus();[m
[32m+[m[32m  });[m
[32m+[m[32m  modal.on('hidden.bs.modal', function() {[m
[32m+[m[32m    localSearchReset(searchSelector, resultSelector);[m
[32m+[m[32m  });[m
[32m+[m[32m})();[m
