[32m+[m[32mHTMLElement.prototype.wrap = function(wrapper) {[m
[32m+[m[32m  this.parentNode.insertBefore(wrapper, this);[m
[32m+[m[32m  this.parentNode.removeChild(this);[m
[32m+[m[32m  wrapper.appendChild(this);[m
[32m+[m[32m};[m
