[1mdiff --git a/themes/fluid/scripts/utils/compare-versions.js b/themes/fluid/scripts/utils/compare-versions.js[m
[1mnew file mode 100644[m
[1mindex 0000000..2726b43[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/scripts/utils/compare-versions.js[m
[36m@@ -0,0 +1,118 @@[m
[32m+[m[32m'use strict';[m
[32m+[m[32m// Ref: https://github.com/omichelsen/compare-versions[m
[32m+[m
[32m+[m[32m/* global define */[m
[32m+[m[32m(function (root, factory) {[m
[32m+[m[32m  /* istanbul ignore next */[m
[32m+[m[32m  if (typeof define === 'function' && define.amd) {[m
[32m+[m[32m    define([], factory);[m
[32m+[m[32m  } else if (typeof exports === 'object') {[m
[32m+[m[32m    module.exports = factory();[m
[32m+[m[32m  } else {[m
[32m+[m[32m    root.compareVersions = factory();[m
[32m+[m[32m  }[m
[32m+[m[32m}(this, function () {[m
[32m+[m
[32m+[m[32m  var semver = /^v?(?:\d+)(\.(?:[x*]|\d+)(\.(?:[x*]|\d+)(\.(?:[x*]|\d+))?(?:-[\da-z\-]+(?:\.[\da-z\-]+)*)?(?:\+[\da-z\-]+(?:\.[\da-z\-]+)*)?)?)?$/i;[m
[32m+[m
[32m+[m[32m  function indexOrEnd(str, q) {[m
[32m+[m[32m    return str.indexOf(q) === -1 ? str.length : str.indexOf(q);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function split(v) {[m
[32m+[m[32m    var c = v.replace(/^v/, '').replace(/\+.*$/, '');[m
[32m+[m[32m    var patchIndex = indexOrEnd(c, '-');[m
[32m+[m[32m    var arr = c.substring(0, patchIndex).split('.');[m
[32m+[m[32m    arr.push(c.substring(patchIndex + 1));[m
[32m+[m[32m    return arr;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function tryParse(v) {[m
[32m+[m[32m    return isNaN(Number(v)) ? v : Number(v);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function validate(version) {[m
[32m+[m[32m    if (typeof version !== 'string') {[m
[32m+[m[32m      throw new TypeError('Invalid argument expected string');[m
[32m+[m[32m    }[m
[32m+[m[32m    if (!semver.test(version)) {[m
[32m+[m[32m      throw new Error('Invalid argument not valid semver (\''+version+'\' received)');[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function compareVersions(v1, v2) {[m
[32m+[m[32m    [v1, v2].forEach(validate);[m
[32m+[m
[32m+[m[32m    var s1 = split(v1);[m
[32m+[m[32m    var s2 = split(v2);[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < Math.max(s1.length - 1, s2.length - 1); i++) {[m
[32m+[m[32m      var n1 = parseInt(s1[i] || 0, 10);[m
[32m+[m[32m      var n2 = parseInt(s2[i] || 0, 10);[m
[32m+[m
[32m+[m[32m      if (n1 > n2) return 1;[m
[32m+[m[32m      if (n2 > n1) return -1;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var sp1 = s1[s1.length - 1];[m
[32m+[m[32m    var sp2 = s2[s2.length - 1];[m
[32m+[m
[32m+[m[32m    if (sp1 && sp2) {[m
[32m+[m[32m      var p1 = sp1.split('.').map(tryParse);[m
[32m+[m[32m      var p2 = sp2.split('.').map(tryParse);[m
[32m+[m
[32m+[m[32m      for (i = 0; i < Math.max(p1.length, p2.length); i++) {[m
[32m+[m[32m        if (p1[i] === undefined || typeof p2[i] === 'string' && typeof p1[i] === 'number') return -1;[m
[32m+[m[32m        if (p2[i] === undefined || typeof p1[i] === 'string' && typeof p2[i] === 'number') return 1;[m
[32m+[m
[32m+[m[32m        if (p1[i] > p2[i]) return 1;[m
[32m+[m[32m        if (p2[i] > p1[i]) return -1;[m
[32m+[m[32m      }[m
[32m+[m[32m    } else if (sp1 || sp2) {[m
[32m+[m[32m      return sp1 ? -1 : 1;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return 0;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  var allowedOperators = [[m
[32m+[m[32m    '>',[m
[32m+[m[32m    '>=',[m
[32m+[m[32m    '=',[m
[32m+[m[32m    '<',[m
[32m+[m[32m    '<='[m
[32m+[m[32m  ];[m
[32m+[m
[32m+[m[32m  var operatorResMap = {[m
[32m+[m[32m    '>': [1],[m
[32m+[m[32m    '>=': [0, 1],[m
[32m+[m[32m    '=': [0],[m
[32m+[m[32m    '<=': [-1, 0],[m
[32m+[m[32m    '<': [-1][m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  function validateOperator(op) {[m
[32m+[m[32m    if (typeof op !== 'string') {[m
[32m+[m[32m      throw new TypeError('Invalid operator type, expected string but got ' + typeof op);[m
[32m+[m[32m    }[m
[32m+[m[32m    if (allowedOperators.indexOf(op) === -1) {[m
[32m+[m[32m      throw new TypeError('Invalid operator, expected one of ' + allowedOperators.join('|'));[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  compareVersions.validate = function(version) {[m
[32m+[m[32m    return typeof version === 'string' && semver.test(version);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  compareVersions.compare = function (v1, v2, operator) {[m
[32m+[m[32m    // Validate operator[m
[32m+[m[32m    validateOperator(operator);[m
[32m+[m
[32m+[m[32m    // since result of compareVersions can only be -1 or 0 or 1[m
[32m+[m[32m    // a simple map can be used to replace switch[m
[32m+[m[32m    var res = compareVersions(v1, v2);[m
[32m+[m[32m    return operatorResMap[operator].indexOf(res) > -1;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return compareVersions;[m
[32m+[m[32m}));[m
[1mdiff --git a/themes/fluid/scripts/utils/crypto.js b/themes/fluid/scripts/utils/crypto.js[m
[1mnew file mode 100644[m
[1mindex 0000000..9f1007a[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/scripts/utils/crypto.js[m
[36m@@ -0,0 +1,9 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst crypto = require('crypto');[m
[32m+[m
[32m+[m[32mconst md5 = (content) => {[m
[32m+[m[32m  return crypto.createHash('md5').update(content).digest('hex');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mmodule.exports = md5;[m
[1mdiff --git a/themes/fluid/scripts/utils/object.js b/themes/fluid/scripts/utils/object.js[m
[1mnew file mode 100644[m
[1mindex 0000000..7e7d870[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/scripts/utils/object.js[m
[36m@@ -0,0 +1,36 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst isObject = (obj) => {[m
[32m+[m[32m  return obj && typeof obj === 'object' && !Array.isArray(obj);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst isNotEmptyObject = (obj) => {[m
[32m+[m[32m  return obj && typeof obj === 'object' && Object.getOwnPropertyNames(obj).length !== 0;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst isEmptyObject = (obj) => {[m
[32m+[m[32m  return !isNotEmptyObject(obj);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst merge = (target, ...sources) => {[m
[32m+[m[32m  for (const source of sources) {[m
[32m+[m[32m    for (const key in source) {[m
[32m+[m[32m      if (!Object.prototype.hasOwnProperty.call(source, key)) {[m
[32m+[m[32m        continue;[m
[32m+[m[32m      }[m
[32m+[m[32m      if (isObject(target[key]) && isObject(source[key])) {[m
[32m+[m[32m        merge(target[key], source[key]);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        target[key] = source[key];[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  return target;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = {[m
[32m+[m[32m  isObject,[m
[32m+[m[32m  isNotEmptyObject,[m
[32m+[m[32m  isEmptyObject,[m
[32m+[m[32m  merge[m
[32m+[m[32m};[m
[1mdiff --git a/themes/fluid/scripts/utils/resolve.js b/themes/fluid/scripts/utils/resolve.js[m
[1mnew file mode 100644[m
[1mindex 0000000..6c78971[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/scripts/utils/resolve.js[m
[36m@@ -0,0 +1,15 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst path = require('path');[m
[32m+[m
[32m+[m[32mconst resolveModule = (name, file = '') => {[m
[32m+[m[32m  let dir;[m
[32m+[m[32m  try {[m
[32m+[m[32m    dir = path.dirname(require.resolve(`${name}/package.json`));[m
[32m+[m[32m  } catch (error) {[m
[32m+[m[32m    return '';[m
[32m+[m[32m  }[m
[32m+[m[32m  return `${dir}/${file}`;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = resolveModule;[m
[1mdiff --git a/themes/fluid/scripts/utils/url-join.js b/themes/fluid/scripts/utils/url-join.js[m
[1mnew file mode 100644[m
[1mindex 0000000..aec2778[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/scripts/utils/url-join.js[m
[36m@@ -0,0 +1,12 @@[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst urlJoin = function(base, relative) {[m
[32m+[m[32m  if (relative && /^https*:\/\//.test(relative)) {[m
[32m+[m[32m    return relative;[m
[32m+[m[32m  }[m
[32m+[m[32m  return relative[m
[32m+[m[32m    ? base.replace(/\/+$/, '') + '/' + relative.replace(/^\/+/, '')[m
[32m+[m[32m    : base;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = urlJoin;[m
