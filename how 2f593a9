[32m+[m[32m    image_caption: theme.post.image_caption,[m
[32m+[m[32m    image_zoom: theme.post.image_zoom,[m
[32m+[m[32m    toc: theme.post.toc,[m
[32m+[m[32m    lazyload: theme.lazyload,[m
[32m+[m[32m    web_analytics: theme.web_analytics,[m
[32m+[m[32m    search_path: urlJoin(config.root, theme.search.path),[m
[32m+[m[32m    include_content_in_search: theme.search.content,[m
[32m+[m[32m  };[m
[32m+[m[32m  return `<script id="fluid-configs">[m
[32m+[m[32m    var Fluid = window.Fluid || {};[m
[32m+[m[32m    Fluid.ctx = Object.assign({}, Fluid.ctx)[m
[32m+[m[32m    var CONFIG = ${JSON.stringify(exportConfig)};[m
[32m+[m
[32m+[m[32m    if (CONFIG.web_analytics.follow_dnt) {[m
[32m+[m[32m      var dntVal = navigator.doNotTrack || window.doNotTrack || navigator.msDoNotTrack;[m
[32m+[m[32m      Fluid.ctx.dnt = dntVal && (dntVal.startsWith('1') || dntVal.startsWith('yes') || dntVal.startsWith('on'));[m
[32m+[m[32m    }[m
[32m+[m[32m  </script>`;[m
[32m+[m[32m});[m
