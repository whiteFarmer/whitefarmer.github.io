[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst fs = require('fs');[m
[32m+[m[32mconst path = require('path');[m
[32m+[m[32mconst objUtil = require('../../utils/object');[m
[32m+[m[32mconst { isNotEmptyObject } = require('../../utils/object');[m
[32m+[m
[32m+[m[32mmodule.exports = (hexo) => {[m
[32m+[m[32m  const isZh = hexo.theme.i18n.languages[0].search(/zh-CN/i) !== -1;[m
[32m+[m
[32m+[m[32m  let dataConfig = {};[m
[32m+[m[32m  let dataStaticConfig = {};[m
[32m+[m
[32m+[m[32m  if (hexo.locals.get instanceof Function) {[m
[32m+[m[32m    const data = hexo.locals.get('data');[m
[32m+[m[32m    if (data && isNotEmptyObject(data.fluid_config)) {[m
[32m+[m[32m      dataConfig = data.fluid_config;[m
[32m+[m[32m    } else if (!configFromRoot(hexo)) {[m
[32m+[m[32m      if (isZh) {[m
[32m+[m[32m        hexo.log.warn('[Fluid] æŽ¨èä½ ä½¿ç”¨è¦†ç›–é…ç½®åŠŸèƒ½: https://hexo.fluid-dev.com/docs/guide/#%E8%A6%86%E7%9B%96%E9%85%8D%E7%BD%AE');[m
[32m+[m[32m      } else {[m
[32m+[m[32m        hexo.log.warn('[Fluid] It is recommended that you use override configuration: https://hexo.fluid-dev.com/docs/en/guide/#override-configuration');[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    if (data && isNotEmptyObject(data.fluid_static_prefix)) {[m
[32m+[m[32m      dataStaticConfig = data.fluid_static_prefix;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    const { language } = hexo.config;[m
[32m+[m[32m    const { i18n } = hexo.theme;[m
[32m+[m[32m    const langConfigMap = {};[m
[32m+[m[32m    for (const key in data) {[m
[32m+[m[32m      if (Object.prototype.hasOwnProperty.call(data, key)) {[m
[32m+[m[32m        if (/^languages\/.+$/.test(key)) {[m
[32m+[m[32m          langConfigMap[key.replace('languages/', '')] = data[key];[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    if (isNotEmptyObject(langConfigMap)) {[m
[32m+[m[32m      const mergeLang = (lang) => {[m
[32m+[m[32m        if (langConfigMap[lang]) {[m
[32m+[m[32m          i18n.set(lang, objUtil.merge({}, i18n.get([lang]), langConfigMap[lang]));[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m[32m      if (Array.isArray(language)) {[m
[32m+[m[32m        for (const lang of language) {[m
[32m+[m[32m          mergeLang(lang);[m
[32m+[m[32m        }[m
[32m+[m[32m      } else {[m
[32m+[m[32m        mergeLang(language);[m
[32m+[m[32m      }[m
[32m+[m[32m      if (isZh) {[m
[32m+[m[32m        hexo.log.debug('[Fluid] è¯»å– source/_data/languages/*.yml æ–‡ä»¶è¦†ç›–è¯­è¨€é…ç½®');[m
[32m+[m[32m      } else {[m
[32m+[m[32m        hexo.log.debug('[Fluid] Merge language config from source/_data/languages/*.yml');[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (isNotEmptyObject(hexo.config.theme_config)) {[m
[32m+[m[32m    hexo.theme.config = objUtil.merge({}, hexo.theme.config, hexo.config.theme_config);[m
[32m+[m[32m    if (isZh) {[m
[32m+[m[32m      hexo.log.debug('[Fluid] è¯»å– _config.yml ä¸­ theme_config é…ç½®é¡¹è¦†ç›–ä¸»é¢˜é…ç½®');[m
[32m+[m[32m    } else {[m
[32m+[m[32m      hexo.log.debug('[Fluid] Merge theme config from theme_config in _config.yml');[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (isNotEmptyObject(dataStaticConfig)) {[m
[32m+[m[32m    hexo.theme.config.static_prefix = objUtil.merge({}, hexo.theme.config.static_prefix, dataStaticConfig);[m
[32m+[m[32m    if (isZh) {[m
[32m+[m[32m      hexo.log.debug('[Fluid] è¯»å– source/_data/fluid_static_prefix.yml æ–‡ä»¶è¦†ç›–ä¸»é¢˜é…ç½®');[m
[32m+[m[32m    } else {[m
[32m+[m[32m      hexo.log.debug('[Fluid] Merge theme config from source/_data/fluid_static_prefix.yml');[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (isNotEmptyObject(dataConfig)) {[m
[32m+[m[32m    hexo.theme.config = objUtil.merge({}, hexo.theme.config, dataConfig);[m
[32m+[m[32m    if (isZh) {[m
[32m+[m[32m      hexo.log.debug('[Fluid] è¯»å– source/_data/fluid_config.yml æ–‡ä»¶è¦†ç›–ä¸»é¢˜é…ç½®');[m
[32m+[m[32m    } else {[m
[32m+[m[32m      hexo.log.debug('[Fluid] Merge theme config from source/_data/fluid_config.yml');[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  hexo.log.debug('[Fluid] Output theme config:\n', JSON.stringify(hexo.theme.config, undefined, 2));[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst configFromRoot = (hexo) => {[m
[32m+[m[32m  const configPath = path.join(hexo.base_dir, '_config.fluid.yml');[m
[32m+[m[32m  return fs.existsSync(configPath);[m
[32m+[m[32m};[m
