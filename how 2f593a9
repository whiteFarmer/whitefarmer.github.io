[32m+[m[32mconst urlJoin = require('../utils/url-join');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Export theme config to js[m
[32m+[m[32m */[m
[32m+[m[32mhexo.extend.helper.register('export_config', function () {[m
[32m+[m[32m  let { config, theme, fluid_version } = this;[m
[32m+[m[32m  const exportConfig = {[m
[32m+[m[32m    hostname: url.parse(config.url).hostname || config.url,[m
[32m+[m[32m    root: config.root,[m
