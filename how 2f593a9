[1mdiff --git a/themes/fluid/scripts/generators/local-search.js b/themes/fluid/scripts/generators/local-search.js[m
[1mnew file mode 100644[m
[1mindex 0000000..eb17acc[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/scripts/generators/local-search.js[m
[36m@@ -0,0 +1,68 @@[m
[32m+[m[32m/* global hexo */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mhexo.extend.generator.register('_hexo_generator_search', function(locals) {[m
[32m+[m[32m  const config = this.theme.config;[m
[32m+[m[32m  if (!config.search.enable) {[m
[32m+[m[32m    return;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  const nunjucks = require('nunjucks');[m
[32m+[m[32m  const env = new nunjucks.Environment();[m
[32m+[m[32m  const pathFn = require('path');[m
[32m+[m[32m  const fs = require('fs');[m
[32m+[m
[32m+[m[32m  env.addFilter('uriencode', function(str) {[m
[32m+[m[32m    return encodeURI(str);[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  env.addFilter('noControlChars', function(str) {[m
[32m+[m[32m    // eslint-disable-next-line no-control-regex[m
[32m+[m[32m    return str && str.replace(/[\x00-\x1F\x7F]/g, '');[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  env.addFilter('urlJoin', function(str) {[m
[32m+[m[32m    const base = str[0];[m
[32m+[m[32m    const relative = str[1];[m
[32m+[m[32m    return relative[m
[32m+[m[32m      ? base.replace(/\/+$/, '') + '/' + relative.replace(/^\/+/, '')[m
[32m+[m[32m      : base;[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  const searchTmplSrc = pathFn.join(hexo.theme_dir, './source/xml/local-search.xml');[m
[32m+[m[32m  const searchTmpl = nunjucks.compile(fs.readFileSync(searchTmplSrc, 'utf8'), env);[m
[32m+[m
[32m+[m[32m  const searchConfig = config.search;[m
[32m+[m[32m  let searchField = searchConfig.field;[m
[32m+[m[32m  const content = searchConfig.content && true;[m
[32m+[m
[32m+[m[32m  let posts, pages;[m
[32m+[m
[32m+[m[32m  if (searchField.trim() !== '') {[m
[32m+[m[32m    searchField = searchField.trim();[m
[32m+[m[32m    if (searchField === 'post') {[m
[32m+[m[32m      posts = locals.posts.sort('-date');[m
[32m+[m[32m    } else if (searchField === 'page') {[m
[32m+[m[32m      pages = locals.pages;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      posts = locals.posts.sort('-date');[m
[32m+[m[32m      pages = locals.pages;[m
[32m+[m[32m    }[m
[32m+[m[32m  } else {[m
[32m+[m[32m    posts = locals.posts.sort('-date');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  const xml = searchTmpl.render({[m
[32m+[m[32m    config : config,[m
[32m+[m[32m    posts  : posts,[m
[32m+[m[32m    pages  : pages,[m
[32m+[m[32m    content: content,[m
[32m+[m[32m    url    : hexo.config.root[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    path: searchConfig.generate_path || '/local-search.xml',[m
[32m+[m[32m    data: xml[m
[32m+[m[32m  };[m
[32m+[m[32m});[m
