[1mdiff --git a/themes/fluid/layout/_partials/comments/giscus.ejs b/themes/fluid/layout/_partials/comments/giscus.ejs[m
[1mnew file mode 100644[m
[1mindex 0000000..97b5cca[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/layout/_partials/comments/giscus.ejs[m
[36m@@ -0,0 +1,36 @@[m
[32m+[m[32m<% if (theme.giscus && theme.giscus['repo'] && theme.giscus['repo-id']) { %>[m
[32m+[m[32m    <div id="giscus" class="giscus"></div>[m
[32m+[m[32m    <script type="text/javascript">[m
[32m+[m[32m      Fluid.utils.loadComments('#giscus', function() {[m
[32m+[m[32m        var options = <%- JSON.stringify(theme.giscus || {}) %>;[m
[32m+[m[32m        var attributes = {};[m
[32m+[m[32m        for (let option in options) {[m
[32m+[m[32m          if (!option.startsWith('theme-')) {[m
[32m+[m[32m            var key = option.startsWith('data-') ? option : 'data-' + option;[m
[32m+[m[32m            attributes[key] = options[option];[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m        var light = '<%= theme.giscus['theme-light'] || 'light' %>';[m
[32m+[m[32m        var dark = '<%= theme.giscus['theme-dark'] || 'dark' %>';[m
[32m+[m[32m        window.GiscusThemeLight = light;[m
[32m+[m[32m        window.GiscusThemeDark = dark;[m
[32m+[m[32m        attributes['data-theme'] = document.documentElement.getAttribute('data-user-color-scheme') === 'dark' ? dark : light;[m
[32m+[m[32m        for (let attribute in attributes) {[m
[32m+[m[32m          var value = attributes[attribute];[m
[32m+[m[32m          if (value === undefined || value === null || value === '') {[m
[32m+[m[32m            delete attributes[attribute];[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m        var s = document.createElement('script');[m
[32m+[m[32m        s.setAttribute('src', 'https://giscus.app/client.js');[m
[32m+[m[32m        s.setAttribute('crossorigin', 'anonymous');[m
[32m+[m[32m        for (let attribute in attributes) {[m
[32m+[m[32m          s.setAttribute(attribute, attributes[attribute]);[m
[32m+[m[32m        }[m
[32m+[m[32m        var ss = document.getElementsByTagName('script');[m
[32m+[m[32m        var e = ss.length > 0 ? ss[ss.length - 1] : document.head || document.documentElement;[m
[32m+[m[32m        e.parentNode.insertBefore(s, e.nextSibling);[m
[32m+[m[32m      });[m
[32m+[m[32m    </script>[m
