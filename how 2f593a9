[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst pagination = require('hexo-pagination');[m
[32m+[m
[32m+[m[32mmodule.exports = function(locals) {[m
[32m+[m[32m  const config = this.config;[m
[32m+[m[32m  const posts = locals.index_posts.sort(config.index_generator.order_by);[m
[32m+[m
[32m+[m[32m  posts.data.sort((a, b) => (b.sticky || 0) - (a.sticky || 0));[m
[32m+[m
[32m+[m[32m  const paginationDir = config.pagination_dir || 'page';[m
[32m+[m[32m  const path = config.index_generator.path || '';[m
[32m+[m
[32m+[m[32m  return pagination(path, posts, {[m
[32m+[m[32m    perPage: config.index_generator.per_page,[m
[32m+[m[32m    layout: 'index',[m
[32m+[m[32m    format: paginationDir + '/%d/',[m
[32m+[m[32m    data: {[m
[32m+[m[32m      __index: true[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
