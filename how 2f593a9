[1mindex 0000000..2ef1e41[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/layout/_partials/plugins/typed.ejs[m
[36m@@ -0,0 +1,52 @@[m
[32m+[m[32m<% if(theme.fun_features.typing.enable && in_scope(theme.fun_features.typing.scope) && page.subtitle !== false) { %>[m
[32m+[m[32m  <%- js_ex(theme.static_prefix.typed, '/typed.min.js') %>[m
[32m+[m[32m  <script>[m
[32m+[m[32m    (function (window, document) {[m
[32m+[m[32m      var typing = Fluid.plugins.typing;[m
[32m+[m[32m      var subtitle = document.getElementById('subtitle');[m
[32m+[m[32m      if (!subtitle || !typing) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m[32m      var text = subtitle.getAttribute('data-typed-text');[m
[32m+[m[32m      <% if (is_home() && theme.index.slogan.api && theme.index.slogan.api.enable) { %>[m
[32m+[m[32m        jQuery.ajax({[m
[32m+[m[32m          type: '<%= theme.index.slogan.api.method %>',[m
[32m+[m[32m          url: '<%- theme.index.slogan.api.url %>',[m
[32m+[m[32m          headers: <%- JSON.stringify(theme.index.slogan.api.headers || {}) %>,[m
[32m+[m[32m          dataType: 'json',[m
[32m+[m[32m          success: function(result) {[m
[32m+[m[32m            var apiText;[m
[32m+[m[32m            if (result) {[m
[32m+[m[32m              var keys = '<%= theme.index.slogan.api.keys %>'.split(',');[m
[32m+[m[32m              if (result instanceof Array) {[m
[32m+[m[32m                result = result[0];[m
[32m+[m[32m              }[m
[32m+[m[32m              for (const k of keys) {[m
[32m+[m[32m                var value = result[k];[m
[32m+[m[32m                if (typeof value === 'string') {[m
[32m+[m[32m                  apiText = value;[m
[32m+[m[32m                  break;[m
[32m+[m[32m                } else if (value instanceof Object) {[m
[32m+[m[32m                  result = value;[m
[32m+[m[32m                }[m
[32m+[m[32m              }[m
[32m+[m[32m            }[m
[32m+[m[32m            apiText ? typing(apiText) : typing(text);[m
[32m+[m[32m          },[m
[32m+[m[32m          error: function(xhr, status, error) {[m
[32m+[m[32m            if (error) {[m
[32m+[m[32m              console.error('Failed to request <%= theme.index.slogan.api.url %>:', error);[m
[32m+[m[32m            }[m
[32m+[m[32m            typing(text);[m
[32m+[m[32m          }[m
[32m+[m[32m        })[m
[32m+[m[32m      <% } else if (Array.isArray(theme.index.slogan.text) && theme.index.slogan.text.length > 0) { %>[m
[32m+[m[32m        var texts = <%- JSON.stringify(theme.index.slogan.text) %>;[m
[32m+[m[32m        var idx = Math.floor(Math.random() * texts.length);[m
[32m+[m[32m        typing(texts[idx]);[m
[32m+[m[32m      <% } else { %>[m
[32m+[m[32m        typing(text);[m
[32m+[m[32m      <% } %>[m
[32m+[m[32m    })(window, document);[m
[32m+[m[32m  </script>[m
[32m+[m[32m<% } %>[m
