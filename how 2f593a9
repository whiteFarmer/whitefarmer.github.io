[1mdiff --git a/themes/fluid/scripts/tags/group-image.js b/themes/fluid/scripts/tags/group-image.js[m
[1mnew file mode 100644[m
[1mindex 0000000..6d640d1[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/scripts/tags/group-image.js[m
[36m@@ -0,0 +1,78 @@[m
[32m+[m[32m/* global hexo */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst DEFAULT_LAYOUTS = {[m
[32m+[m[32m  2 : [1, 1],[m
[32m+[m[32m  3 : [2, 1],[m
[32m+[m[32m  4 : [2, 2],[m
[32m+[m[32m  5 : [3, 2],[m
[32m+[m[32m  6 : [3, 3],[m
[32m+[m[32m  7 : [3, 2, 2],[m
[32m+[m[32m  8 : [3, 2, 3],[m
[32m+[m[32m  9 : [3, 3, 3],[m
[32m+[m[32m  10: [3, 2, 2, 3][m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst groupBy = (total, layout) => {[m
[32m+[m[32m  const r = [];[m
[32m+[m[32m  for (let count of total) {[m
[32m+[m[32m    r.push(layout.slice(0, count));[m
[32m+[m[32m    layout = layout.slice(count);[m
[32m+[m[32m  }[m
[32m+[m[32m  return r;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst templates = {[m
[32m+[m
[32m+[m[32m  dispatch: (images, total, layout) => {[m
[32m+[m[32m    const valid = layout && (layout.reduce((prev, current) => prev + current) === total);[m
[32m+[m[32m    const _layout = valid ? layout : DEFAULT_LAYOUTS[total];[m
[32m+[m[32m    return _layout ? templates.getHTML(groupBy(_layout, images)) : templates.defaults(images);[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  defaults: (images) => {[m
[32m+[m[32m    const ROW_SIZE = 3;[m
[32m+[m[32m    const rows = images.length / ROW_SIZE;[m
[32m+[m[32m    const imageArr = [];[m
[32m+[m
[32m+[m[32m    for (let i = 0; i < rows; i++) {[m
[32m+[m[32m      imageArr.push(images.slice(i * ROW_SIZE, (i + 1) * ROW_SIZE));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return templates.getHTML(imageArr);[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  getHTML: (rows) => {[m
[32m+[m[32m    return rows.map(row => {[m
[32m+[m[32m      return `<div class="group-image-row">${templates.getColumnHTML(row)}</div>`;[m
[32m+[m[32m    }).join('');[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  getColumnHTML: (images) => {[m
[32m+[m[32m    return images.map(image => {[m
[32m+[m[32m      return `<div class="group-image-wrap">${image}</div>`;[m
[32m+[m[32m    }).join('');[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst groupImage = (args, content) => {[m
[32m+[m[32m  const total = parseInt(args[0], 10);[m
[32m+[m[32m  const layout = args[1] && args[1].split('-').map((v) => parseInt(v, 10));[m
[32m+[m
[32m+[m[32m  content = hexo.render.renderSync({ text: content, engine: 'markdown' });[m
[32m+[m
[32m+[m[32m  const images = content.match(/<img[\s\S]*?>/g);[m
[32m+[m
[32m+[m[32m  return `<div class="group-image-container">${templates.dispatch(images, total, layout)}</div>`;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m  {% groupimage total n1-n2-n3-... %}[m
[32m+[m[32m  ![](url)[m
[32m+[m[32m  ![](url)[m
[32m+[m[32m  ![](url)[m
[32m+[m[32m  {% endgroupimage %}[m
[32m+[m[32m */[m
[32m+[m[32mhexo.extend.tag.register('groupimage', groupImage, { ends: true });[m
[32m+[m[32mhexo.extend.tag.register('gi', groupImage, { ends: true });[m
