[32m+[m[32m  const disabled = (args[3] || '').length > 0 && args[3].trim() !== 'false';[m
[32m+[m[32m  const content = hexo.render.renderSync({ text: text, engine: 'markdown' }).replace(/(<p>)|(<\/p>)/g, '').replace(/<br>/g, '');[m
[32m+[m
[32m+[m[32m  return `${!inline ? '<div>' : ''}[m
[32m+[m[32m            <input type="checkbox" ${disabled ? 'disabled' : ''} ${checked ? 'checked="checked"' : ''}>${content}[m
[32m+[m[32m          ${!inline ? '</div>' : ''}`;[m
[32m+[m
[32m+[m[32m};[m
[32m+[m
