[32m+[m[32m// ä»Žé…ç½®æ–‡ä»¶ä¸­èŽ·å– umami çš„é…ç½®[m
[32m+[m[32mconst website_id = CONFIG.web_analytics.umami.website_id;[m
[32m+[m[32m// æ‹¼æŽ¥è¯·æ±‚åœ°å€[m
[32m+[m[32mconst request_url = `${CONFIG.web_analytics.umami.api_server}/api/websites/${website_id}/stats`;[m
[32m+[m
[32m+[m[32mconst start_time = new Date(CONFIG.web_analytics.umami.start_time).getTime();[m
[32m+[m[32mconst end_time = new Date().getTime();[m
[32m+[m[32mconst token = CONFIG.web_analytics.umami.token;[m
[32m+[m
[32m+[m[32m// æ£€æŸ¥é…ç½®æ˜¯å¦ä¸ºç©º[m
[32m+[m[32mif (!website_id) {[m
[32m+[m[32m  throw new Error("Umami website_id is empty");[m
[32m+[m[32m}[m
[32m+[m[32mif (!request_url) {[m
[32m+[m[32m  throw new Error("Umami request_url is empty");[m
[32m+[m[32m}[m
[32m+[m[32mif (!start_time) {[m
[32m+[m[32m  throw new Error("Umami start_time is empty");[m
[32m+[m[32m}[m
[32m+[m[32mif (!token) {[m
[32m+[m[32m  throw new Error("Umami token is empty");[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// æž„é€ è¯·æ±‚å‚æ•°[m
[32m+[m[32mconst params = new URLSearchParams({[m
[32m+[m[32m  startAt: start_time,[m
[32m+[m[32m  endAt: end_time,[m
[32m+[m[32m});[m
[32m+[m[32m// æž„é€ è¯·æ±‚å¤´[m
[32m+[m[32mconst request_header = {[m
[32m+[m[32m  method: "GET",[m
[32m+[m[32m  headers: {[m
[32m+[m[32m    "Content-Type": "application/json",[m
[32m+[m[32m    "Authorization": "Bearer " + token,[m
[32m+[m[32m  },[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// èŽ·å–ç«™ç‚¹ç»Ÿè®¡æ•°æ®[m
[32m+[m[32masync function siteStats() {[m
[32m+[m[32m  try {[m
[32m+[m[32m    const response = await fetch(`${request_url}?${params}`, request_header);[m
[32m+[m[32m    const data = await response.json();[m
[32m+[m[32m    const uniqueVisitors = data.visitors.value; // èŽ·å–ç‹¬ç«‹è®¿å®¢æ•°[m
[32m+[m[32m    const pageViews = data.pageviews.value; // èŽ·å–é¡µé¢æµè§ˆé‡[m
[32m+[m
[32m+[m[32m    let pvCtn = document.querySelector("#umami-site-pv-container");[m
[32m+[m[32m    if (pvCtn) {[m
[32m+[m[32m      let ele = document.querySelector("#umami-site-pv");[m
[32m+[m[32m      if (ele) {[m
[32m+[m[32m        ele.textContent = pageViews; // è®¾ç½®é¡µé¢æµè§ˆé‡[m
[32m+[m[32m        pvCtn.style.display = "inline"; // å°†å…ƒç´ æ˜¾ç¤ºå‡ºæ¥[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    let uvCtn = document.querySelector("#umami-site-uv-container");[m
[32m+[m[32m    if (uvCtn) {[m
[32m+[m[32m      let ele = document.querySelector("#umami-site-uv");[m
[32m+[m[32m      if (ele) {[m
[32m+[m[32m        ele.textContent = uniqueVisitors;[m
[32m+[m[32m        uvCtn.style.display = "inline";[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  } catch (error) {[m
[32m+[m[32m    console.error(error);[m
[32m+[m[32m    return "-1";[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// èŽ·å–é¡µé¢æµè§ˆé‡[m
[32m+[m[32masync function pageStats(path) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    const response = await fetch(`${request_url}?${params}&url=${path}`, request_header);[m
[32m+[m[32m    const data = await response.json();[m
[32m+[m[32m    const pageViews = data.pageviews.value;[m
[32m+[m
[32m+[m[32m    let viewCtn = document.querySelector("#umami-page-views-container");[m
[32m+[m[32m    if (viewCtn) {[m
[32m+[m[32m      let ele = document.querySelector("#umami-page-views");[m
[32m+[m[32m      if (ele) {[m
[32m+[m[32m        ele.textContent = pageViews;[m
[32m+[m[32m        viewCtn.style.display = "inline";[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  } catch (error) {[m
[32m+[m[32m    console.error(error);[m
[32m+[m[32m    return "-1";[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32msiteStats();[m
[32m+[m
[32m+[m[32m// èŽ·å–é¡µé¢å®¹å™¨[m
[32m+[m[32mlet viewCtn = document.querySelector("#umami-page-views-container");[m
[32m+[m[32m// å¦‚æžœé¡µé¢å®¹å™¨å­˜åœ¨ï¼Œåˆ™èŽ·å–é¡µé¢æµè§ˆé‡[m
[32m+[m[32mif (viewCtn) {[m
[32m+[m[32m  let path = window.location.pathname;[m
[32m+[m[32m  let target = decodeURI(path.replace(/\/*(index.html)?$/, "/"));[m
[32m+[m[32m  pageStats(target);[m
[32m+[m[32m}[m
