[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mlet css;[m
[32m+[m[32mtry {[m
[32m+[m[32m  css = require('css');[m
[32m+[m[32m} catch (error) {[m
[32m+[m[32m  if (error.code === 'MODULE_NOT_FOUND') {[m
[32m+[m[32m    css = require('@adobe/css-tools');[m
[32m+[m[32m  } else {[m
[32m+[m[32m    throw error;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mconst fs = require('fs');[m
[32m+[m[32mconst objUtil = require('../../utils/object');[m
[32m+[m[32mconst resolveModule = require('../../utils/resolve');[m
[32m+[m
[32m+[m[32mmodule.exports = (hexo) => {[m
[32m+[m
[32m+[m[32m  function resolveHighlight(name) {[m
[32m+[m[32m    if (!name) {[m
[32m+[m[32m      name = 'github-gist';[m
[32m+[m[32m    }[m
[32m+[m[32m    const cssName = name.toLowerCase().replace(/([^0-9])\s+?([^0-9])/g, '$1-$2').replace(/\s/g, '');[m
[32m+[m[32m    let file = resolveModule('highlight.js', `styles/${cssName}.css`);[m
[32m+[m[32m    if (cssName === 'github-gist' && !fs.existsSync(file)) {[m
[32m+[m[32m      file = resolveModule('highlight.js', 'styles/github.css');[m
[32m+[m[32m    }[m
[32m+[m[32m    let backgroundColor;[m
[32m+[m[32m    if (fs.existsSync(file)) {[m
[32m+[m[32m      const content = fs.readFileSync(file, 'utf8');[m
[32m+[m[32m      css.parse(content).stylesheet.rules[m
[32m+[m[32m        .filter(rule => rule.type === 'rule' && rule.selectors.some(selector => selector.endsWith('.hljs')))[m
[32m+[m[32m        .flatMap(rule => rule.declarations)[m
[32m+[m[32m        .forEach(declaration => {[m
[32m+[m[32m          if (declaration.property === 'background' || declaration.property === 'background-color') {[m
[32m+[m[32m            backgroundColor = declaration.value;[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m    } else {[m
[32m+[m[32m      hexo.log.error(`[Fluid] highlightjs style '${name}' not found`);[m
[32m+[m[32m      return {};[m
[32m+[m[32m    }[m
[32m+[m[32m    if (backgroundColor === 'white' || backgroundColor === '#ffffff') {[m
[32m+[m[32m      backgroundColor = '#fff';[m
[32m+[m[32m    }[m
[32m+[m[32m    return { file, backgroundColor };[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function resolvePrism(name) {[m
[32m+[m[32m    if (!name) {[m
[32m+[m[32m      name = 'default';[m
[32m+[m[32m    }[m
[32m+[m[32m    let cssName = name.toLowerCase().replace(/[\s-]/g, '');[m
[32m+[m[32m    if (cssName === 'prism' || cssName === 'default') {[m
[32m+[m[32m      cssName = '';[m
[32m+[m[32m    } else if (cssName === 'tomorrownight') {[m
[32m+[m[32m      cssName = 'tomorrow';[m
[32m+[m[32m    }[m
[32m+[m[32m    let file = resolveModule('prismjs', `themes/${cssName ? 'prism-' + cssName : 'prism'}.css`);[m
[32m+[m[32m    if (!fs.existsSync(file)) {[m
[32m+[m[32m      file = resolveModule('prism-themes', `themes/${cssName}.css`);[m
[32m+[m[32m    }[m
[32m+[m[32m    if (!fs.existsSync(file)) {[m
[32m+[m[32m      hexo.log.error(`[Fluid] prismjs style '${name}' not found`);[m
[32m+[m[32m      return {};[m
[32m+[m[32m    }[m
[32m+[m[32m    return { file };[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  const config = hexo.theme.config;[m
[32m+[m[32m  if (!config.code || !config.code.highlight.enable) {[m
[32m+[m[32m    return;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (config.code.highlight.lib === 'highlightjs') {[m
[32m+[m[32m    // Force set hexo config[m
[32m+[m[32m    hexo.config.prismjs = objUtil.merge({}, hexo.config.prismjs, {[m
[32m+[m[32m      enable: false[m
[32m+[m[32m    });[m
[32m+[m[32m    hexo.config.highlight = objUtil.merge({}, hexo.config.highlight, {[m
[32m+[m[32m      enable     : true,[m
[32m+[m[32m      hljs       : true,[m
[32m+[m[32m      wrap       : false,[m
[32m+[m[32m      auto_detect: true,[m
[32m+[m[32m      line_number: config.code.highlight.line_number || false[m
[32m+[m[32m    });[m
[32m+[m[32m    hexo.config.syntax_highlighter = 'highlight.js';  // hexo v7.0.0+ config[m
[32m+[m[32m    hexo.theme.config.code.highlight.highlightjs = objUtil.merge({}, hexo.theme.config.code.highlight.highlightjs, {[m
[32m+[m[32m      light: resolveHighlight(hexo.theme.config.code.highlight.highlightjs.style),[m
[32m+[m[32m      dark : hexo.theme.config.dark_mode.enable && resolveHighlight(hexo.theme.config.code.highlight.highlightjs.style_dark)[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    hexo.extend.filter.register('after_post_render', (page) => {[m
[32m+[m[32m      if (hexo.config.highlight.line_number) {[m
[32m+[m[32m        page.content = page.content.replace(/<figure[^>]+?highlight.+?<td[^>]+?code[^>]+?>.*?(<pre.+?<\/pre>).+?<\/figure>/gims, (str, p1) => {[m
[32m+[m[32m          if (/<code[^>]+?mermaid[^>]+?>/ims.test(p1)) {[m
[32m+[m[32m            return p1.replace(/(class="[^>]*?)hljs([^>]*?")/gims, '$1$2').replace(/<br>/gims, '\n');[m
[32m+[m[32m          }[m
[32m+[m[32m          return str;[m
[32m+[m[32m        });[m
[32m+[m[32m      } else {[m
[32m+[m[32m        page.content = page.content.replace(/(?<!<div class="code-wrapper">)<pre.+?<\/pre>/gims, (str) => {[m
[32m+[m[32m          if (/<code[^>]+?mermaid[^>]+?>/ims.test(str)) {[m
[32m+[m[32m            return str.replace(/(class=".*?)hljs(.*?")/gims, '$1$2');[m
[32m+[m[32m          }[m
[32m+[m[32m          return `<div class="code-wrapper">${str}</div>`;[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // é€‚é…ç¼©è¿›åž‹ä»£ç å—[m
[32m+[m[32m      page.content = page.content.replace(/<pre><code>/gims, (str) => {[m
[32m+[m[32m        return '<pre><code class="hljs">';[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      return page;[m
[32m+[m[32m    });[m
[32m+[m[32m  } else if (config.code.highlight.lib === 'prismjs') {[m
[32m+[m[32m    // Force set hexo config[m
[32m+[m[32m    hexo.config.highlight = objUtil.merge({}, hexo.config.highlight, {[m
[32m+[m[32m      enable: false[m
[32m+[m[32m    });[m
[32m+[m[32m    hexo.config.prismjs = objUtil.merge({}, hexo.config.prismjs, {[m
[32m+[m[32m      enable     : true,[m
[32m+[m[32m      preprocess : config.code.highlight.prismjs.preprocess || false,[m
[32m+[m[32m      line_number: config.code.highlight.line_number || false[m
[32m+[m[32m    });[m
[32m+[m[32m    hexo.config.syntax_highlighter = 'prismjs';  // hexo v7.0.0+ config[m
[32m+[m[32m    hexo.theme.config.code.highlight.prismjs = objUtil.merge({}, hexo.theme.config.code.highlight.prismjs, {[m
[32m+[m[32m      light: resolvePrism(hexo.theme.config.code.highlight.prismjs.style),[m
[32m+[m[32m      dark : hexo.theme.config.dark_mode.enable && resolvePrism(hexo.theme.config.code.highlight.prismjs.style_dark)[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    hexo.extend.filter.register('after_post_render', (page) => {[m
[32m+[m[32m      page.content = page.content.replace(/(?<!<div class="code-wrapper">)<pre.+?<\/pre>/gims, (str) => {[m
[32m+[m[32m        if (/<code[^>]+?mermaid[^>]+?>/ims.test(str)) {[m
[32m+[m[32m          if (hexo.config.highlight.line_number) {[m
[32m+[m[32m            str = str.replace(/<span[^>]+?line-numbers-rows[^>]+?>.+?<\/span><\/code>/, '</code>');[m
[32m+[m[32m          }[m
[32m+[m[32m          str = str.replace(/<pre[^>]*?>/gims, '<pre>')[m
[32m+[m[32m            .replace(/(class=".*?)language-mermaid(.*?")/gims, '$1mermaid$2')[m
[32m+[m[32m            .replace(/<span[^>]+?>(.+?)<\/span>/gims, '$1');[m
[32m+[m[32m          return str;[m
[32m+[m[32m        }[m
[32m+[m[32m        return `<figure><div class="code-wrapper">${str}</div></figure>`;[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      // é€‚é…ç¼©è¿›åž‹ä»£ç å—[m
[32m+[m[32m      page.content = page.content.replace(/<pre><code>/gims, (str) => {[m
[32m+[m[32m        return '<pre class="language-none"><code class="language-none">';[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      return page;[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
