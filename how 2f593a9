[1mnew file mode 100644[m
[1mindex 0000000..d61bc26[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/source/js/utils.js[m
[36m@@ -0,0 +1,245 @@[m
[32m+[m[32m/* global Fluid, CONFIG */[m
[32m+[m
[32m+[m[32mwindow.requestAnimationFrame = window.requestAnimationFrame || window.webkitRequestAnimationFrame || window.mozRequestAnimationFrame;[m
[32m+[m
[32m+[m[32mFluid.utils = {[m
[32m+[m
[32m+[m[32m  listenScroll: function(callback) {[m
[32m+[m[32m    var dbc = new Debouncer(callback);[m
[32m+[m[32m    window.addEventListener('scroll', dbc, false);[m
[32m+[m[32m    dbc.handleEvent();[m
[32m+[m[32m    return dbc;[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  unlistenScroll: function(callback) {[m
[32m+[m[32m    window.removeEventListener('scroll', callback);[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  listenDOMLoaded(callback) {[m
[32m+[m[32m    if (document.readyState !== 'loading') {[m
[32m+[m[32m      callback();[m
[32m+[m[32m    } else {[m
[32m+[m[32m      document.addEventListener('DOMContentLoaded', function () {[m
[32m+[m[32m        callback();[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  scrollToElement: function(target, offset) {[m
[32m+[m[32m    var of = jQuery(target).offset();[m
[32m+[m[32m    if (of) {[m
[32m+[m[32m      jQuery('html,body').animate({[m
[32m+[m[32m        scrollTop: of.top + (offset || 0),[m
[32m+[m[32m        easing   : 'swing'[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  elementVisible: function(element, offsetFactor) {[m
[32m+[m[32m    offsetFactor = offsetFactor && offsetFactor >= 0 ? offsetFactor : 0;[m
[32m+[m[32m    var rect = element.getBoundingClientRect();[m
[32m+[m[32m    const viewportHeight = window.innerHeight || document.documentElement.clientHeight;[m
[32m+[m[32m    return ([m
[32m+[m[32m      (rect.top >= 0 && rect.top <= viewportHeight * (1 + offsetFactor) + rect.height / 2) ||[m
[32m+[m[32m      (rect.bottom >= 0 && rect.bottom <= viewportHeight * (1 + offsetFactor) + rect.height / 2)[m
[32m+[m[32m    );[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  waitElementVisible: function(selectorOrElement, callback, offsetFactor) {[m
[32m+[m[32m    var runningOnBrowser = typeof window !== 'undefined';[m
[32m+[m[32m    var isBot = (runningOnBrowser && !('onscroll' in window))[m
[32m+[m[32m      || (typeof navigator !== 'undefined' && /(gle|ing|ro|msn)bot|crawl|spider|yand|duckgo/i.test(navigator.userAgent));[m
[32m+[m[32m    if (!runningOnBrowser || isBot) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    offsetFactor = offsetFactor && offsetFactor >= 0 ? offsetFactor : 0;[m
[32m+[m
[32m+[m[32m    function waitInViewport(element) {[m
[32m+[m[32m      Fluid.utils.listenDOMLoaded(function() {[m
[32m+[m[32m        if (Fluid.utils.elementVisible(element, offsetFactor)) {[m
[32m+[m[32m          callback();[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m[32m        if ('IntersectionObserver' in window) {[m
[32m+[m[32m          var io = new IntersectionObserver(function(entries, ob) {[m
[32m+[m[32m            if (entries[0].isIntersecting) {[m
[32m+[m[32m              callback();[m
[32m+[m[32m              ob.disconnect();[m
[32m+[m[32m            }[m
[32m+[m[32m          }, {[m
[32m+[m[32m            threshold : [0],[m
[32m+[m[32m            rootMargin: (window.innerHeight || document.documentElement.clientHeight) * offsetFactor + 'px'[m
[32m+[m[32m          });[m
[32m+[m[32m          io.observe(element);[m
[32m+[m[32m        } else {[m
[32m+[m[32m          var wrapper = Fluid.utils.listenScroll(function() {[m
[32m+[m[32m            if (Fluid.utils.elementVisible(element, offsetFactor)) {[m
[32m+[m[32m              Fluid.utils.unlistenScroll(wrapper);[m
[32m+[m[32m              callback();[m
[32m+[m[32m            }[m
[32m+[m[32m          });[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (typeof selectorOrElement === 'string') {[m
[32m+[m[32m      this.waitElementLoaded(selectorOrElement, function(element) {[m
[32m+[m[32m        waitInViewport(element);[m
[32m+[m[32m      });[m
[32m+[m[32m    } else {[m
[32m+[m[32m      waitInViewport(selectorOrElement);[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  waitElementLoaded: function(selector, callback) {[m
[32m+[m[32m    var runningOnBrowser = typeof window !== 'undefined';[m
[32m+[m[32m    var isBot = (runningOnBrowser && !('onscroll' in window))[m
[32m+[m[32m      || (typeof navigator !== 'undefined' && /(gle|ing|ro|msn)bot|crawl|spider|yand|duckgo/i.test(navigator.userAgent));[m
[32m+[m[32m    if (!runningOnBrowser || isBot) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if ('MutationObserver' in window) {[m
[32m+[m[32m      var mo = new MutationObserver(function(records, ob) {[m
[32m+[m[32m        var ele = document.querySelector(selector);[m
[32m+[m[32m        if (ele) {[m
[32m+[m[32m          callback(ele);[m
[32m+[m[32m          ob.disconnect();[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m      mo.observe(document, { childList: true, subtree: true });[m
[32m+[m[32m    } else {[m
[32m+[m[32m      Fluid.utils.listenDOMLoaded(function() {[m
[32m+[m[32m        var waitLoop = function() {[m
[32m+[m[32m          var ele = document.querySelector(selector);[m
[32m+[m[32m          if (ele) {[m
[32m+[m[32m            callback(ele);[m
[32m+[m[32m          } else {[m
[32m+[m[32m            setTimeout(waitLoop, 100);[m
[32m+[m[32m          }[m
[32m+[m[32m        };[m
[32m+[m[32m        waitLoop();[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  createScript: function(url, onload) {[m
[32m+[m[32m    var s = document.createElement('script');[m
[32m+[m[32m    s.setAttribute('src', url);[m
[32m+[m[32m    s.setAttribute('type', 'text/javascript');[m
[32m+[m[32m    s.setAttribute('charset', 'UTF-8');[m
[32m+[m[32m    s.async = false;[m
[32m+[m[32m    if (typeof onload === 'function') {[m
[32m+[m[32m      if (window.attachEvent) {[m
[32m+[m[32m        s.onreadystatechange = function() {[m
[32m+[m[32m          var e = s.readyState;[m
[32m+[m[32m          if (e === 'loaded' || e === 'complete') {[m
[32m+[m[32m            s.onreadystatechange = null;[m
[32m+[m[32m            onload();[m
[32m+[m[32m          }[m
[32m+[m[32m        };[m
[32m+[m[32m      } else {[m
[32m+[m[32m        s.onload = onload;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    var ss = document.getElementsByTagName('script');[m
[32m+[m[32m    var e = ss.length > 0 ? ss[ss.length - 1] : document.head || document.documentElement;[m
[32m+[m[32m    e.parentNode.insertBefore(s, e.nextSibling);[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  createCssLink: function(url) {[m
[32m+[m[32m    var l = document.createElement('link');[m
[32m+[m[32m    l.setAttribute('rel', 'stylesheet');[m
[32m+[m[32m    l.setAttribute('type', 'text/css');[m
[32m+[m[32m    l.setAttribute('href', url);[m
[32m+[m[32m    var e = document.getElementsByTagName('link')[0][m
[32m+[m[32m      || document.getElementsByTagName('head')[0][m
[32m+[m[32m      || document.head || document.documentElement;[m
[32m+[m[32m    e.parentNode.insertBefore(l, e);[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  loadComments: function(selector, loadFunc) {[m
[32m+[m[32m    var ele = document.querySelector('#comments[lazyload]');[m
[32m+[m[32m    if (ele) {[m
[32m+[m[32m      var callback = function() {[m
[32m+[m[32m        loadFunc();[m
[32m+[m[32m        ele.removeAttribute('lazyload');[m
[32m+[m[32m      };[m
[32m+[m[32m      Fluid.utils.waitElementVisible(selector, callback, CONFIG.lazyload.offset_factor);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      loadFunc();[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  getBackgroundLightness(selectorOrElement) {[m
[32m+[m[32m    var ele = selectorOrElement;[m
[32m+[m[32m    if (typeof selectorOrElement === 'string') {[m
[32m+[m[32m      ele = document.querySelector(selectorOrElement);[m
[32m+[m[32m    }[m
[32m+[m[32m    var view = ele.ownerDocument.defaultView;[m
[32m+[m[32m    if (!view) {[m
[32m+[m[32m      view = window;[m
[32m+[m[32m    }[m
[32m+[m[32m    var rgbArr = view.getComputedStyle(ele).backgroundColor.replace(/rgba*\(/, '').replace(')', '').split(/,\s*/);[m
[32m+[m[32m    if (rgbArr.length < 3) {[m
[32m+[m[32m      return 0;[m
[32m+[m[32m    }[m
[32m+[m[32m    var colorCast = (0.213 * rgbArr[0]) + (0.715 * rgbArr[1]) + (0.072 * rgbArr[2]);[m
[32m+[m[32m    return colorCast === 0 || colorCast > 255 / 2 ? 1 : -1;[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  retry(handler, interval, times) {[m
[32m+[m[32m    if (times <= 0) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m[32m    var next = function() {[m
[32m+[m[32m      if (--times >= 0 && !handler()) {[m
[32m+[m[32m        setTimeout(next, interval);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m[32m    setTimeout(next, interval);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Handles debouncing of events via requestAnimationFrame[m
[32m+[m[32m * @see http://www.html5rocks.com/en/tutorials/speed/animations/[m
[32m+[m[32m * @param {Function} callback The callback to handle whichever event[m
[32m+[m[32m */[m
[32m+[m[32mfunction Debouncer(callback) {[m
[32m+[m[32m  this.callback = callback;[m
[32m+[m[32m  this.ticking = false;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mDebouncer.prototype = {[m
[32m+[m[32m  constructor: Debouncer,[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * dispatches the event to the supplied callback[m
[32m+[m[32m   * @private[m
[32m+[m[32m   */[m
[32m+[m[32m  update: function() {[m
[32m+[m[32m    this.callback && this.callback();[m
[32m+[m[32m    this.ticking = false;[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * ensures events don't get stacked[m
[32m+[m[32m   * @private[m
[32m+[m[32m   */[m
[32m+[m[32m  requestTick: function() {[m
[32m+[m[32m    if (!this.ticking) {[m
[32m+[m[32m      requestAnimationFrame(this.rafCallback || (this.rafCallback = this.update.bind(this)));[m
[32m+[m[32m      this.ticking = true;[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Attach this as the event listeners[m
[32m+[m[32m   */[m
[32m+[m[32m  handleEvent: function() {[m
[32m+[m[32m    this.requestTick();[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
