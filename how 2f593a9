[32m+[m[32m * Modified from https://blog.skk.moe/post/hello-darkmode-my-old-friend/[m
[32m+[m[32m */[m
[32m+[m[32m(function(window, document) {[m
[32m+[m[32m  var rootElement = document.documentElement;[m
[32m+[m[32m  var colorSchemaStorageKey = 'Fluid_Color_Scheme';[m
[32m+[m[32m  var colorSchemaMediaQueryKey = '--color-mode';[m
[32m+[m[32m  var userColorSchemaAttributeName = 'data-user-color-scheme';[m
[32m+[m[32m  var defaultColorSchemaAttributeName = 'data-default-color-scheme';[m
[32m+[m[32m  var colorToggleButtonSelector = '#color-toggle-btn';[m
[32m+[m[32m  var colorToggleIconSelector = '#color-toggle-icon';[m
[32m+[m[32m  var iframeSelector = 'iframe';[m
[32m+[m
[32m+[m[32m  function setLS(k, v) {[m
[32m+[m[32m    try {[m
[32m+[m[32m      localStorage.setItem(k, v);[m
[32m+[m[32m    } catch (e) {}[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function removeLS(k) {[m
[32m+[m[32m    try {[m
[32m+[m[32m      localStorage.removeItem(k);[m
[32m+[m[32m    } catch (e) {}[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function getLS(k) {[m
[32m+[m[32m    try {[m
[32m+[m[32m      return localStorage.getItem(k);[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      return null;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function getSchemaFromHTML() {[m
[32m+[m[32m    var res = rootElement.getAttribute(defaultColorSchemaAttributeName);[m
[32m+[m[32m    if (typeof res === 'string') {[m
[32m+[m[32m      return res.replace(/["'\s]/g, '');[m
[32m+[m[32m    }[m
[32m+[m[32m    return null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function getSchemaFromCSSMediaQuery() {[m
[32m+[m[32m    var res = getComputedStyle(rootElement).getPropertyValue([m
[32m+[m[32m      colorSchemaMediaQueryKey[m
[32m+[m[32m    );[m
[32m+[m[32m    if (typeof res === 'string') {[m
[32m+[m[32m      return res.replace(/["'\s]/g, '');[m
[32m+[m[32m    }[m
[32m+[m[32m    return null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function resetSchemaAttributeAndLS() {[m
[32m+[m[32m    rootElement.setAttribute(userColorSchemaAttributeName, getDefaultColorSchema());[m
[32m+[m[32m    removeLS(colorSchemaStorageKey);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var validColorSchemaKeys = {[m
[32m+[m[32m    dark : true,[m
[32m+[m[32m    light: true[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  function getDefaultColorSchema() {[m
[32m+[m[32m    // å–é»˜è®¤å­—æ®µçš„å€¼[m
[32m+[m[32m    var schema = getSchemaFromHTML();[m
[32m+[m[32m    // å¦‚æžœæ˜Žç¡®æŒ‡å®šäº† schema åˆ™è¿”å›ž[m
[32m+[m[32m    if (validColorSchemaKeys[schema]) {[m
[32m+[m[32m      return schema;[m
[32m+[m[32m    }[m
[32m+[m[32m    // é»˜è®¤ä¼˜å…ˆæŒ‰ prefers-color-scheme[m
[32m+[m[32m    schema = getSchemaFromCSSMediaQuery();[m
[32m+[m[32m    if (validColorSchemaKeys[schema]) {[m
[32m+[m[32m      return schema;[m
[32m+[m[32m    }[m
[32m+[m[32m    // å¦åˆ™æŒ‰æœ¬åœ°æ—¶é—´æ˜¯å¦å¤§äºŽ 18 ç‚¹æˆ–å‡Œæ™¨ 0 ~ 6 ç‚¹[m
[32m+[m[32m    var hours = new Date().getHours();[m
[32m+[m[32m    if (hours >= 18 || (hours >= 0 && hours <= 6)) {[m
[32m+[m[32m      return 'dark';[m
[32m+[m[32m    }[m
[32m+[m[32m    return 'light';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function applyCustomColorSchemaSettings(schema) {[m
[32m+[m[32m    // æŽ¥å—ä»Žã€Œå¼€å…³ã€å¤„ä¼ æ¥çš„æ¨¡å¼ï¼Œæˆ–è€…ä»Ž localStorage è¯»å–ï¼Œå¦åˆ™æŒ‰é»˜è®¤è®¾ç½®å€¼[m
[32m+[m[32m    var current = schema || getLS(colorSchemaStorageKey) || getDefaultColorSchema();[m
[32m+[m
[32m+[m[32m    if (current === getDefaultColorSchema()) {[m
[32m+[m[32m      // å½“ç”¨æˆ·åˆ‡æ¢çš„æ˜¾ç¤ºæ¨¡å¼å’Œé»˜è®¤æ¨¡å¼ç›¸åŒæ—¶ï¼Œåˆ™æ¢å¤ä¸ºè‡ªåŠ¨æ¨¡å¼[m
[32m+[m[32m      resetSchemaAttributeAndLS();[m
[32m+[m[32m    } else if (validColorSchemaKeys[current]) {[m
[32m+[m[32m      rootElement.setAttribute([m
[32m+[m[32m        userColorSchemaAttributeName,[m
[32m+[m[32m        current[m
[32m+[m[32m      );[m
[32m+[m[32m    } else {[m
[32m+[m[32m      // ç‰¹æ®Šæƒ…å†µé‡ç½®[m
[32m+[m[32m      resetSchemaAttributeAndLS();[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // æ ¹æ®å½“å‰æ¨¡å¼è®¾ç½®å›¾æ ‡[m
[32m+[m[32m    setButtonIcon(current);[m
[32m+[m
[32m+[m[32m    // è®¾ç½®ä»£ç é«˜äº®[m
[32m+[m[32m    setHighlightCSS(current);[m
[32m+[m
[32m+[m[32m    // è®¾ç½®å…¶ä»–åº”ç”¨[m
[32m+[m[32m    setApplications(current);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var invertColorSchemaObj = {[m
[32m+[m[32m    dark : 'light',[m
[32m+[m[32m    light: 'dark'[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  function getIconClass(scheme) {[m
[32m+[m[32m    return 'icon-' + scheme;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function toggleCustomColorSchema() {[m
[32m+[m[32m    var currentSetting = getLS(colorSchemaStorageKey);[m
[32m+[m
[32m+[m[32m    if (validColorSchemaKeys[currentSetting]) {[m
[32m+[m[32m      // ä»Ž localStorage ä¸­è¯»å–æ¨¡å¼ï¼Œå¹¶å–ç›¸åçš„æ¨¡å¼[m
[32m+[m[32m      currentSetting = invertColorSchemaObj[currentSetting];[m
[32m+[m[32m    } else if (currentSetting === null) {[m
[32m+[m[32m      // å½“ localStorage ä¸­æ²¡æœ‰ç›¸å…³å€¼ï¼Œæˆ–è€… localStorage æŠ›äº† Error[m
[32m+[m[32m      // å…ˆæŒ‰ç…§æŒ‰é’®çš„çŠ¶æ€è¿›è¡Œåˆ‡æ¢[m
[32m+[m[32m      var iconElement = document.querySelector(colorToggleIconSelector);[m
[32m+[m[32m      if (iconElement) {[m
[32m+[m[32m        currentSetting = iconElement.getAttribute('data');[m
[32m+[m[32m      }[m
[32m+[m[32m      if (!iconElement || !validColorSchemaKeys[currentSetting]) {[m
[32m+[m[32m        // å½“ localStorage ä¸­æ²¡æœ‰ç›¸å…³å€¼ï¼Œæˆ–è€… localStorage æŠ›äº† Errorï¼Œåˆ™è¯»å–é»˜è®¤å€¼å¹¶åˆ‡æ¢åˆ°ç›¸åçš„æ¨¡å¼[m
[32m+[m[32m        currentSetting = invertColorSchemaObj[getSchemaFromCSSMediaQuery()];[m
[32m+[m[32m      }[m
[32m+[m[32m    } else {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m[32m    // å°†ç›¸åçš„æ¨¡å¼å†™å…¥ localStorage[m
[32m+[m[32m    setLS(colorSchemaStorageKey, currentSetting);[m
[32m+[m
[32m+[m[32m    return currentSetting;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function setButtonIcon(schema) {[m
[32m+[m[32m    if (validColorSchemaKeys[schema]) {[m
[32m+[m[32m      // åˆ‡æ¢å›¾æ ‡[m
[32m+[m[32m      var icon = getIconClass('dark');[m
[32m+[m[32m      if (schema) {[m
[32m+[m[32m        icon = getIconClass(schema);[m
[32m+[m[32m      }[m
[32m+[m[32m      var iconElement = document.querySelector(colorToggleIconSelector);[m
[32m+[m[32m      if (iconElement) {[m
[32m+[m[32m        iconElement.setAttribute([m
[32m+[m[32m          'class',[m
[32m+[m[32m          'iconfont ' + icon[m
[32m+[m[32m        );[m
[32m+[m[32m        iconElement.setAttribute([m
[32m+[m[32m          'data',[m
[32m+[m[32m          invertColorSchemaObj[schema][m
[32m+[m[32m        );[m
[32m+[m[32m      } else {[m
[32m+[m[32m        // å¦‚æžœå›¾æ ‡ä¸å­˜åœ¨åˆ™è¯´æ˜Žå›¾æ ‡è¿˜æ²¡åŠ è½½å‡ºæ¥ï¼Œç­‰åˆ°é¡µé¢å…¨éƒ¨åŠ è½½å†å°è¯•åˆ‡æ¢[m
[32m+[m[32m        Fluid.utils.waitElementLoaded(colorToggleIconSelector, function() {[m
[32m+[m[32m          var iconElement = document.querySelector(colorToggleIconSelector);[m
[32m+[m[32m          if (iconElement) {[m
[32m+[m[32m            iconElement.setAttribute([m
[32m+[m[32m              'class',[m
[32m+[m[32m              'iconfont ' + icon[m
[32m+[m[32m            );[m
[32m+[m[32m            iconElement.setAttribute([m
[32m+[m[32m              'data',[m
[32m+[m[32m              invertColorSchemaObj[schema][m
[32m+[m[32m            );[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m[32m      if (document.documentElement.getAttribute('data-user-color-scheme')) {[m
[32m+[m[32m        var color = getComputedStyle(document.documentElement).getPropertyValue('--navbar-bg-color').trim()[m
[32m+[m[32m        document.querySelector('meta[name="theme-color"]').setAttribute('content', color)[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function setHighlightCSS(schema) {[m
[32m+[m[32m    // å¯ç”¨å¯¹åº”çš„ä»£ç é«˜äº®çš„æ ·å¼[m
[32m+[m[32m    var lightCss = document.getElementById('highlight-css');[m
[32m+[m[32m    var darkCss = document.getElementById('highlight-css-dark');[m
[32m+[m[32m    if (schema === 'dark') {[m
[32m+[m[32m      if (darkCss) {[m
[32m+[m[32m        darkCss.removeAttribute('disabled');[m
[32m+[m[32m      }[m
[32m+[m[32m      if (lightCss) {[m
[32m+[m[32m        lightCss.setAttribute('disabled', '');[m
[32m+[m[32m      }[m
[32m+[m[32m    } else {[m
[32m+[m[32m      if (lightCss) {[m
[32m+[m[32m        lightCss.removeAttribute('disabled');[m
[32m+[m[32m      }[m
[32m+[m[32m      if (darkCss) {[m
[32m+[m[32m        darkCss.setAttribute('disabled', '');[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    setTimeout(function() {[m
[32m+[m[32m      // è®¾ç½®ä»£ç å—ç»„ä»¶æ ·å¼[m
[32m+[m[32m      document.querySelectorAll('.markdown-body pre').forEach((pre) => {[m
[32m+[m[32m        var cls = Fluid.utils.getBackgroundLightness(pre) >= 0 ? 'code-widget-light' : 'code-widget-dark';[m
[32m+[m[32m        var widget = pre.querySelector('.code-widget-light, .code-widget-dark');[m
[32m+[m[32m        if (widget) {[m
[32m+[m[32m          widget.classList.remove('code-widget-light', 'code-widget-dark');[m
[32m+[m[32m          widget.classList.add(cls);[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    }, 200);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function setApplications(schema) {[m
[32m+[m[32m    // è®¾ç½® remark42 è¯„è®ºä¸»é¢˜[m
[32m+[m[32m    if (window.REMARK42) {[m
[32m+[m[32m      window.REMARK42.changeTheme(schema);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // è®¾ç½® cusdis è¯„è®ºä¸»é¢˜[m
[32m+[m[32m    if (window.CUSDIS) {[m
[32m+[m[32m      window.CUSDIS.setTheme(schema);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // è®¾ç½® utterances è¯„è®ºä¸»é¢˜[m
[32m+[m[32m    var utterances = document.querySelector('.utterances-frame');[m
[32m+[m[32m    if (utterances) {[m
[32m+[m[32m      var utterancesTheme = schema === 'dark' ? window.UtterancesThemeDark : window.UtterancesThemeLight;[m
[32m+[m[32m      const message = {[m
[32m+[m[32m        type : 'set-theme',[m
[32m+[m[32m        theme: utterancesTheme[m
[32m+[m[32m      };[m
[32m+[m[32m      utterances.contentWindow.postMessage(message, 'https://utteranc.es');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // è®¾ç½® giscus è¯„è®ºä¸»é¢˜[m
[32m+[m[32m    var giscus = document.querySelector('iframe.giscus-frame');[m
[32m+[m[32m    if (giscus) {[m
[32m+[m[32m      var giscusTheme = schema === 'dark' ? window.GiscusThemeDark : window.GiscusThemeLight;[m
[32m+[m[32m      const message = {[m
[32m+[m[32m        setConfig: {[m
[32m+[m[32m          theme: giscusTheme,[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m[32m      // giscus.style.cssText += 'color-scheme: normal;';[m
[32m+[m[32m      giscus.contentWindow.postMessage({ 'giscus': message }, 'https://giscus.app');[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // å½“é¡µé¢åŠ è½½æ—¶ï¼Œå°†æ˜¾ç¤ºæ¨¡å¼è®¾ç½®ä¸º localStorage ä¸­è‡ªå®šä¹‰çš„å€¼ï¼ˆå¦‚æžœæœ‰çš„è¯ï¼‰[m
[32m+[m[32m  applyCustomColorSchemaSettings();[m
[32m+[m
[32m+[m[32m  Fluid.utils.waitElementLoaded(colorToggleIconSelector, function() {[m
[32m+[m[32m    applyCustomColorSchemaSettings();[m
[32m+[m[32m    var button = document.querySelector(colorToggleButtonSelector);[m
[32m+[m[32m    if (button) {[m
[32m+[m[32m      // å½“ç”¨æˆ·ç‚¹å‡»åˆ‡æ¢æŒ‰é’®æ—¶ï¼ŒèŽ·å¾—æ–°çš„æ˜¾ç¤ºæ¨¡å¼ã€å†™å…¥ localStorageã€å¹¶åœ¨é¡µé¢ä¸Šç”Ÿæ•ˆ[m
[32m+[m[32m      button.addEventListener('click', function() {[m
[32m+[m[32m        applyCustomColorSchemaSettings(toggleCustomColorSchema());[m
[32m+[m[32m      });[m
[32m+[m[32m      var icon = document.querySelector(colorToggleIconSelector);[m
[32m+[m[32m      if (icon) {[m
[32m+[m[32m        // å…‰æ ‡æ‚¬åœåœ¨æŒ‰é’®ä¸Šæ—¶ï¼Œåˆ‡æ¢å›¾æ ‡[m
[32m+[m[32m        button.addEventListener('mouseenter', function() {[m
[32m+[m[32m          var current = icon.getAttribute('data');[m
[32m+[m[32m          icon.classList.replace(getIconClass(invertColorSchemaObj[current]), getIconClass(current));[m
[32m+[m[32m        });[m
[32m+[m[32m        button.addEventListener('mouseleave', function() {[m
[32m+[m[32m          var current = icon.getAttribute('data');[m
[32m+[m[32m          icon.classList.replace(getIconClass(current), getIconClass(invertColorSchemaObj[current]));[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  Fluid.utils.waitElementLoaded(iframeSelector, function() {[m
[32m+[m[32m    applyCustomColorSchemaSettings();[m
[32m+[m[32m  });[m
[32m+[m[41m  [m
[32m+[m[32m})(window, document);[m
