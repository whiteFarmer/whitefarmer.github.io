[32m+[m[32m  if (!next) {[m
[32m+[m[32m    return null;[m
[32m+[m[32m  }[m
[32m+[m[32m  if (next.hide) {[m
[32m+[m[32m    return next_post(next);[m
[32m+[m[32m  }[m
[32m+[m[32m  return next;[m
[32m+[m[32m});[m
[1mdiff --git a/themes/fluid/scripts/helpers/scope.js b/themes/fluid/scripts/helpers/scope.js[m
[1mnew file mode 100644[m
[1mindex 0000000..c2264d7[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/scripts/helpers/scope.js[m
[36m@@ -0,0 +1,50 @@[m
[32m+[m[32m/* global hexo */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst pageInScope = (page, scope) => {[m
[32m+[m[32m  switch (scope) {[m
[32m+[m[32m    case 'home':[m
[32m+[m[32m      return Boolean(page.__index);[m
[32m+[m[32m    case 'post':[m
[32m+[m[32m    case 'posts':[m
[32m+[m[32m      return Boolean(page.__post);[m
[32m+[m[32m    case 'archives':[m
[32m+[m[32m    case 'archive':[m
[32m+[m[32m      return Boolean(page.archive);[m
[32m+[m[32m    case 'categories':[m
[32m+[m[32m    case 'category':[m
[32m+[m[32m      return page.layout === 'categories' || page.layout === 'category';[m
[32m+[m[32m    case 'tags':[m
[32m+[m[32m    case 'tag':[m
[32m+[m[32m      return page.layout === 'tags' || page.layout === 'tag';[m
[32m+[m[32m    case 'about':[m
[32m+[m[32m      return page.layout === 'about';[m
[32m+[m[32m    case 'links':[m
[32m+[m[32m    case 'link':[m
[32m+[m[32m      return page.layout === 'links';[m
[32m+[m[32m    case '404':[m
[32m+[m[32m      return page.layout === '404';[m
[32m+[m[32m    case 'page':[m
[32m+[m[32m    case 'custom':[m
[32m+[m[32m      return Boolean(page.__page);[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mhexo.extend.helper.register('in_scope', function(scope) {[m
[32m+[m[32m  if (!scope || scope.length === 0) {[m
[32m+[m[32m    return true;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (Array.isArray(scope)) {[m
[32m+[m[32m    for (const each of scope) {[m
[32m+[m[32m      if (pageInScope(this.page, each)) {[m
[32m+[m[32m        return true;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  } else {[m
[32m+[m[32m    return pageInScope(this.page, scope);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return false;[m
[32m+[m[32m});[m
