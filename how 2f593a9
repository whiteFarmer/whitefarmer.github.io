[32m+[m
[32m+[m[32mconst urlJoin = require('../utils/url-join');[m
[32m+[m
[32m+[m[32mhexo.extend.helper.register('css_ex', function(base, relative, ex = '') {[m
[32m+[m[32m  return `<link ${ex} rel="stylesheet" href="${this.url_for(urlJoin(base, relative))}" />`;[m
[32m+[m[32m});[m
[32m+[m
