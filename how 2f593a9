[32m+[m[32m  typing: function(text) {[m
[32m+[m[32m    if (!('Typed' in window)) { return; }[m
[32m+[m
[32m+[m[32m    var typed = new window.Typed('#subtitle', {[m
[32m+[m[32m      strings: [[m
[32m+[m[32m        '  ',[m
[32m+[m[32m        text[m
[32m+[m[32m      ],[m
[32m+[m[32m      cursorChar: CONFIG.typing.cursorChar,[m
[32m+[m[32m      typeSpeed : CONFIG.typing.typeSpeed,[m
[32m+[m[32m      loop      : CONFIG.typing.loop[m
[32m+[m[32m    });[m
[32m+[m[32m    typed.stop();[m
[32m+[m[32m    var subtitle = document.getElementById('subtitle');[m
[32m+[m[32m    if (subtitle) {[m
[32m+[m[32m      subtitle.innerText = '';[m
[32m+[m[32m    }[m
[32m+[m[32m    jQuery(document).ready(function() {[m
[32m+[m[32m      typed.start();[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  fancyBox: function(selector) {[m
[32m+[m[32m    if (!CONFIG.image_zoom.enable || !('fancybox' in jQuery)) { return; }[m
[32m+[m
[32m+[m[32m    jQuery(selector || '.markdown-body :not(a) > img, .markdown-body > img').each(function() {[m
[32m+[m[32m      var $image = jQuery(this);[m
[32m+[m[32m      var imageUrl = $image.attr('data-src') || $image.attr('src') || '';[m
[32m+[m[32m      if (CONFIG.image_zoom.img_url_replace) {[m
[32m+[m[32m        var rep = CONFIG.image_zoom.img_url_replace;[m
[32m+[m[32m        var r1 = rep[0] || '';[m
[32m+[m[32m        var r2 = rep[1] || '';[m
[32m+[m[32m        if (r1) {[m
[32m+[m[32m          if (/^re:/.test(r1)) {[m
[32m+[m[32m            r1 = r1.replace(/^re:/, '');[m
[32m+[m[32m            var reg = new RegExp(r1, 'gi');[m
[32m+[m[32m            imageUrl = imageUrl.replace(reg, r2);[m
[32m+[m[32m          } else {[m
[32m+[m[32m            imageUrl = imageUrl.replace(r1, r2);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      var $imageWrap = $image.wrap(`[m
[32m+[m[32m        <a class="fancybox fancybox.image" href="${imageUrl}"[m
[32m+[m[32m          itemscope itemtype="http://schema.org/ImageObject" itemprop="url"></a>`[m
[32m+[m[32m      ).parent('a');[m
[32m+[m[32m      if ($imageWrap.length !== 0) {[m
[32m+[m[32m        if ($image.is('.group-image-container img')) {[m
[32m+[m[32m          $imageWrap.attr('data-fancybox', 'group').attr('rel', 'group');[m
[32m+[m[32m        } else {[m
[32m+[m[32m          $imageWrap.attr('data-fancybox', 'default').attr('rel', 'default');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var imageTitle = $image.attr('title') || $image.attr('alt');[m
[32m+[m[32m        if (imageTitle) {[m
[32m+[m[32m          $imageWrap.attr('title', imageTitle).attr('data-caption', imageTitle);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    jQuery.fancybox.defaults.hash = false;[m
[32m+[m[32m    jQuery('.fancybox').fancybox({[m
[32m+[m[32m      loop   : true,[m
[32m+[m[32m      helpers: {[m
[32m+[m[32m        overlay: {[m
[32m+[m[32m          locked: false[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  imageCaption: function(selector) {[m
[32m+[m[32m    if (!CONFIG.image_caption.enable) { return; }[m
[32m+[m
[32m+[m[32m    jQuery(selector || `.markdown-body > p > img, .markdown-body > figure > img,[m
[32m+[m[32m      .markdown-body > p > a.fancybox, .markdown-body > figure > a.fancybox`).each(function() {[m
[32m+[m[32m      var $target = jQuery(this);[m
[32m+[m[32m      var $figcaption = $target.next('figcaption');[m
[32m+[m[32m      if ($figcaption.length !== 0) {[m
[32m+[m[32m        $figcaption.addClass('image-caption');[m
[32m+[m[32m      } else {[m
[32m+[m[32m        var imageTitle = $target.attr('title') || $target.attr('alt');[m
[32m+[m[32m        if (imageTitle) {[m
[32m+[m[32m          $target.after(`<figcaption aria-hidden="true" class="image-caption">${imageTitle}</figcaption>`);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  codeWidget() {[m
[32m+[m[32m    var enableLang = CONFIG.code_language.enable && CONFIG.code_language.default;[m
[32m+[m[32m    var enableCopy = CONFIG.copy_btn && 'ClipboardJS' in window;[m
[32m+[m[32m    if (!enableLang && !enableCopy) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function getBgClass(ele) {[m
[32m+[m[32m      return Fluid.utils.getBackgroundLightness(ele) >= 0 ? 'code-widget-light' : 'code-widget-dark';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var copyTmpl = '';[m
[32m+[m[32m    copyTmpl += '<div class="code-widget">';[m
[32m+[m[32m    copyTmpl += 'LANG';[m
[32m+[m[32m    copyTmpl += '</div>';[m
[32m+[m[32m    jQuery('.markdown-body pre').each(function() {[m
[32m+[m[32m      var $pre = jQuery(this);[m
[32m+[m[32m      if ($pre.find('code.mermaid').length > 0) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m[32m      if ($pre.find('span.line').length > 0) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var lang = '';[m
[32m+[m
[32m+[m[32m      if (enableLang) {[m
[32m+[m[32m        lang = CONFIG.code_language.default;[m
[32m+[m[32m        if ($pre[0].children.length > 0 && $pre[0].children[0].classList.length >= 2 && $pre.children().hasClass('hljs')) {[m
[32m+[m[32m          lang = $pre[0].children[0].classList[1];[m
[32m+[m[32m        } else if ($pre[0].getAttribute('data-language')) {[m
[32m+[m[32m          lang = $pre[0].getAttribute('data-language');[m
[32m+[m[32m        } else if ($pre.parent().hasClass('sourceCode') && $pre[0].children.length > 0 && $pre[0].children[0].classList.length >= 2) {[m
[32m+[m[32m          lang = $pre[0].children[0].classList[1];[m
[32m+[m[32m          $pre.parent().addClass('code-wrapper');[m
[32m+[m[32m        } else if ($pre.parent().hasClass('markdown-body') && $pre[0].classList.length === 0) {[m
[32m+[m[32m          $pre.wrap('<div class="code-wrapper"></div>');[m
[32m+[m[32m        }[m
[32m+[m[32m        lang = lang.toUpperCase().replace('NONE', CONFIG.code_language.default);[m
[32m+[m[32m      }[m
[32m+[m[32m      $pre.append(copyTmpl.replace('LANG', lang).replace('code-widget">',[m
[32m+[m[32m        getBgClass($pre[0]) + (enableCopy ? ' code-widget copy-btn" data-clipboard-snippet><i class="iconfont icon-copy"></i>' : ' code-widget">')));[m
[32m+[m
[32m+[m[32m      if (enableCopy) {[m
[32m+[m[32m        var clipboard = new ClipboardJS('.copy-btn', {[m
[32m+[m[32m          target: function(trigger) {[m
[32m+[m[32m            var nodes = trigger.parentNode.childNodes;[m
[32m+[m[32m            for (var i = 0; i < nodes.length; i++) {[m
[32m+[m[32m              if (nodes[i].tagName === 'CODE') {[m
[32m+[m[32m                return nodes[i];[m
[32m+[m[32m              }[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m        clipboard.on('success', function(e) {[m
[32m+[m[32m          e.clearSelection();[m
[32m+[m[32m          e.trigger.innerHTML = e.trigger.innerHTML.replace('icon-copy', 'icon-success');[m
[32m+[m[32m          setTimeout(function() {[m
[32m+[m[32m            e.trigger.innerHTML = e.trigger.innerHTML.replace('icon-success', 'icon-copy');[m
[32m+[m[32m          }, 2000);[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
