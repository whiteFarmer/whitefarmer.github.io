[32m+[m[32m/* global hexo */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst path = require('path');[m
[32m+[m
[32m+[m[32mhexo.extend.filter.register('template_locals', locals => {[m
[32m+[m[32m  const { env, config } = hexo;[m
[32m+[m[32m  const { __ } = locals;[m
[32m+[m[32m  const { i18n } = hexo.theme;[m
[32m+[m[32m  locals.hexo_version = env.version;[m
[32m+[m[32m  locals.fluid_version = require(path.normalize(path.join(hexo.theme_dir, 'package.json'))).version;[m
[32m+[m[32m  locals.title = __('title') !== 'title' ? __('title') : config.title;[m
[32m+[m[32m  locals.subtitle = __('subtitle') !== 'subtitle' ? __('subtitle') : config.subtitle;[m
[32m+[m[32m  locals.author = __('author') !== 'author' ? __('author') : config.author;[m
[32m+[m[32m  locals.description = __('description') !== 'description' ? __('description') : config.description;[m
[32m+[m[32m  locals.languages = [...i18n.languages];[m
[32m+[m[32m  locals.languages.splice(locals.languages.indexOf('default'), 1);[m
[32m+[m[32m  locals.page.lang = locals.page.lang || locals.page.language;[m
[32m+[m[32m});[m
