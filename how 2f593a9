[32m+[m
[32m+[m[32m  publish-npm:[m
[32m+[m[32m    needs: build[m
[32m+[m[32m    runs-on: ubuntu-latest[m
[32m+[m[32m    steps:[m
[32m+[m[32m      - uses: actions/checkout@v3[m
[32m+[m[32m      - uses: actions/setup-node@v3[m
[32m+[m[32m        with:[m
[32m+[m[32m          node-version: 14[m
[32m+[m[32m          registry-url: https://registry.npmjs.org/[m
[32m+[m[32m      - run: npm publish[m
[32m+[m[32m        env:[m
[32m+[m[32m          NODE_AUTH_TOKEN: ${{secrets.NPM_TOKEN}}[m
