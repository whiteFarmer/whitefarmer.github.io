[1mnew file mode 100644[m
[1mindex 0000000..e0175e1[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/scripts/tags/button.js[m
[36m@@ -0,0 +1,18 @@[m
[32m+[m[32m/* global hexo */[m
[32m+[m
