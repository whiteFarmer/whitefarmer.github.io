[32m+[m[32m  args = args.join(' ').split(',');[m
[32m+[m[32m  const url = (args[0] || '').trim();[m
[32m+[m[32m  const text = (args[1] || '').trim();[m
[32m+[m[32m  const title = (args[2] || '').trim();[m
