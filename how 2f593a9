[32m+[m[32mhexo.extend.helper.register('prev_post', function prev_post(post) {[m
[32m+[m[32m  const prev = post.prev;[m
[32m+[m[32m  if (!prev) {[m
[32m+[m[32m    return null;[m
[32m+[m[32m  }[m
[32m+[m[32m  if (prev.hide) {[m
[32m+[m[32m    return prev_post(prev);[m
[32m+[m[32m  }[m
[32m+[m[32m  return prev;[m
[32m+[m[32m});[m
