[1mdiff --git a/themes/fluid/scripts/helpers/date.js b/themes/fluid/scripts/helpers/date.js[m
[1mnew file mode 100644[m
[1mindex 0000000..2f7c8d8[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/scripts/helpers/date.js[m
[36m@@ -0,0 +1,25 @@[m
[32m+[m[32m/* global hexo */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst moment = require('moment');[m
[32m+[m[32mconst { isMoment } = moment;[m
[32m+[m
[32m+[m[32mhexo.extend.helper.register('compare_date', function(date1, date2) {[m
[32m+[m[32m  if (!date1) {[m
[32m+[m[32m    return -1;[m
[32m+[m[32m  }[m
[32m+[m[32m  if (!date2) {[m
[32m+[m[32m    return 1;[m
[32m+[m[32m  }[m
[32m+[m[32m  const m1 = isMoment(date1) ? date1 : moment(date1);[m
[32m+[m[32m  const m2 = isMoment(date2) ? date2 : moment(date2);[m
[32m+[m[32m  const diff = m1.diff(m2);[m
[32m+[m[32m  if (diff < 0) {[m
[32m+[m[32m    return -1;[m
[32m+[m[32m  } else if (diff > 0) {[m
[32m+[m[32m    return 1;[m
[32m+[m[32m  } else {[m
[32m+[m[32m    return 0;[m
[32m+[m[32m  }[m
[32m+[m[32m});[m
