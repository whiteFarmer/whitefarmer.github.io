[32m+[m[32m/* global hexo */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m// ç”Ÿæˆå‰è¿‡æ»¤æ–‡ç« [m
[32m+[m[32mhexo.extend.filter.register('before_generate', function() {[m
[32m+[m[32m  this._bindLocals();[m
[32m+[m
[32m+[m[32m  const allPages = this.locals.get('pages');[m
[32m+[m[32m  allPages.data.map((page) => {[m
[32m+[m[32m    if (page.comment !== true) {[m
[32m+[m[32m      page.comments = typeof page.comment === 'string' && page.comment !== '';[m
[32m+[m[32m    } else {[m
[32m+[m[32m      page.comments = true;[m
[32m+[m[32m    }[m
[32m+[m[32m    return page;[m
[32m+[m[32m  });[m
[32m+[m[32m  this.locals.set('pages', allPages);[m
[32m+[m
[32m+[m[32m  const allPosts = this.locals.get('posts');[m
[32m+[m[32m  allPosts.data.map((post) => {[m
[32m+[m[32m    if (post.comment === false) {[m
[32m+[m[32m      post.comments = false;[m
[32m+[m[32m    }[m
[32m+[m[32m    return post;[m
[32m+[m[32m  });[m
[32m+[m[32m  const hidePosts = allPosts.filter(post => post.hide);[m
[32m+[m[32m  const normalPosts = allPosts.filter(post => !post.hide);[m
[32m+[m[32m  const indexPost = allPosts.filter(post => !post.hide && !post.archive)[m
[32m+[m
[32m+[m[32m  this.locals.set('all_posts', allPosts);[m
[32m+[m[32m  this.locals.set('hide_posts', hidePosts);[m
[32m+[m[32m  this.locals.set('posts', normalPosts);[m
[32m+[m[32m  this.locals.set('index_posts', indexPost);[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mconst original_post_generator = hexo.extend.generator.get('post');[m
[32m+[m
[32m+[m[32mhexo.extend.generator.register('post', function(locals) {[m
[32m+[m[32m  // å‘é€æ—¶éœ€è¦æŠŠè¿‡æ»¤çš„é¡µé¢ä¹ŸåŠ å…¥[m
[32m+[m[32m  return original_post_generator.bind(this)({[m
[32m+[m[32m    posts: new locals.posts.constructor([m
[32m+[m[32m      locals.posts.data.concat(locals.hide_posts.data)[m
[32m+[m[32m    )[m
[32m+[m[32m  });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// æ¸²æŸ“æ–‡ç« åŽçš„è¿‡æ»¤[m
[32m+[m[32mhexo.extend.filter.register('after_post_render', (page) => {[m
[32m+[m[32m  // ç§»é™¤ hexo-renderer-pandoc ç”Ÿæˆçš„ <colgroup>[m
[32m+[m[32m  page.content = page.content.replace(/<colgroup>.+?<\/colgroup>/gims, '');[m
[32m+[m[32m  // ç§»é™¤ hexo-renderer-pandoc ç”Ÿæˆçš„ <span class="footnote-text">...<br>...</span>[m
[32m+[m[32m  page.content = page.content.replace(/(class="footnote-text".+?)<br.+?>(.+?rev="footnote")/gims, '$1$2');[m
[32m+[m[32m  return page;[m
[32m+[m[32m});[m
