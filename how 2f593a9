[32m+[m[32mHTMLElement.prototype.wrap = function(wrapper) {[m
[32m+[m[32m  this.parentNode.insertBefore(wrapper, this);[m
[32m+[m[32m  this.parentNode.removeChild(this);[m
[32m+[m[32m  wrapper.appendChild(this);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mFluid.events = {[m
[32m+[m
[32m+[m[32m  registerNavbarEvent: function() {[m
[32m+[m[32m    var navbar = jQuery('#navbar');[m
[32m+[m[32m    if (navbar.length === 0) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m[32m    var submenu = jQuery('#navbar .dropdown-menu');[m
[32m+[m[32m    if (navbar.offset().top > 0) {[m
[32m+[m[32m      navbar.removeClass('navbar-dark');[m
[32m+[m[32m      submenu.removeClass('navbar-dark');[m
[32m+[m[32m    }[m
[32m+[m[32m    Fluid.utils.listenScroll(function() {[m
[32m+[m[32m      navbar[navbar.offset().top > 50 ? 'addClass' : 'removeClass']('top-nav-collapse');[m
[32m+[m[32m      submenu[navbar.offset().top > 50 ? 'addClass' : 'removeClass']('dropdown-collapse');[m
[32m+[m[32m      if (navbar.offset().top > 0) {[m
[32m+[m[32m        navbar.removeClass('navbar-dark');[m
[32m+[m[32m        submenu.removeClass('navbar-dark');[m
[32m+[m[32m      } else {[m
[32m+[m[32m        navbar.addClass('navbar-dark');[m
[32m+[m[32m        submenu.removeClass('navbar-dark');[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m    jQuery('#navbar-toggler-btn').on('click', function() {[m
[32m+[m[32m      var $this = jQuery(this);[m
[32m+[m[32m      if ($this.data('animating')) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m[32m      $this.data('animating', true);[m
[32m+[m[32m      jQuery('.animated-icon').toggleClass('open');[m
[32m+[m[32m      jQuery('#navbar').toggleClass('navbar-col-show');[m
[32m+[m[32m      setTimeout(function() {[m
[32m+[m[32m        $this.data('animating', false);[m
[32m+[m[32m      }, 300);[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  registerParallaxEvent: function() {[m
[32m+[m[32m    var ph = jQuery('#banner[parallax="true"]');[m
[32m+[m[32m    if (ph.length === 0) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m[32m    var board = jQuery('#board');[m
[32m+[m[32m    if (board.length === 0) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m[32m    var parallax = function() {[m
[32m+[m[32m      var pxv = jQuery(window).scrollTop() / 5;[m
[32m+[m[32m      var offset = parseInt(board.css('margin-top'), 10);[m
[32m+[m[32m      var max = 96 + offset;[m
[32m+[m[32m      if (pxv > max) {[m
[32m+[m[32m        pxv = max;[m
[32m+[m[32m      }[m
[32m+[m[32m      ph.css({[m
[32m+[m[32m        transform: 'translate3d(0,' + pxv + 'px,0)'[m
[32m+[m[32m      });[m
[32m+[m[32m      var sideCol = jQuery('.side-col');[m
[32m+[m[32m      if (sideCol) {[m
[32m+[m[32m        sideCol.css({[m
[32m+[m[32m          'padding-top': pxv + 'px'[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m[32m    Fluid.utils.listenScroll(parallax);[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  registerScrollDownArrowEvent: function() {[m
[32m+[m[32m    var scrollbar = jQuery('.scroll-down-bar');[m
[32m+[m[32m    if (scrollbar.length === 0) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m[32m    scrollbar.on('click', function() {[m
[32m+[m[32m      Fluid.utils.scrollToElement('#board', -jQuery('#navbar').height());[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  registerScrollTopArrowEvent: function() {[m
[32m+[m[32m    var topArrow = jQuery('#scroll-top-button');[m
[32m+[m[32m    if (topArrow.length === 0) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m[32m    var board = jQuery('#board');[m
[32m+[m[32m    if (board.length === 0) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m[32m    var posDisplay = false;[m
[32m+[m[32m    var scrollDisplay = false;[m
[32m+[m[32m    // Position[m
[32m+[m[32m    var setTopArrowPos = function() {[m
[32m+[m[32m      var boardRight = board[0].getClientRects()[0].right;[m
[32m+[m[32m      var bodyWidth = document.body.offsetWidth;[m
[32m+[m[32m      var right = bodyWidth - boardRight;[m
[32m+[m[32m      posDisplay = right >= 50;[m
[32m+[m[32m      topArrow.css({[m
[32m+[m[32m        'bottom': posDisplay && scrollDisplay ? '20px' : '-60px',[m
[32m+[m[32m        'right' : right - 64 + 'px'[m
[32m+[m[32m      });[m
[32m+[m[32m    };[m
[32m+[m[32m    setTopArrowPos();[m
[32m+[m[32m    jQuery(window).resize(setTopArrowPos);[m
[32m+[m[32m    // Display[m
[32m+[m[32m    var headerHeight = board.offset().top;[m
[32m+[m[32m    Fluid.utils.listenScroll(function() {[m
[32m+[m[32m      var scrollHeight = document.body.scrollTop + document.documentElement.scrollTop;[m
[32m+[m[32m      scrollDisplay = scrollHeight >= headerHeight;[m
[32m+[m[32m      topArrow.css({[m
[32m+[m[32m        'bottom': posDisplay && scrollDisplay ? '20px' : '-60px'[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m[32m    // Click[m
[32m+[m[32m    topArrow.on('click', function() {[m
[32m+[m[32m      jQuery('body,html').animate({[m
[32m+[m[32m        scrollTop: 0,[m
[32m+[m[32m        easing   : 'swing'[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  registerImageLoadedEvent: function() {[m
[32m+[m[32m    if (!('NProgress' in window)) { return; }[m
[32m+[m
[32m+[m[32m    var bg = document.getElementById('banner');[m
[32m+[m[32m    if (bg) {[m
[32m+[m[32m      var src = bg.style.backgroundImage;[m
[32m+[m[32m      var url = src.match(/\((.*?)\)/)[1].replace(/(['"])/g, '');[m
[32m+[m[32m      var img = new Image();[m
[32m+[m[32m      img.onload = function() {[m
[32m+[m[32m        window.NProgress && window.NProgress.status !== null && window.NProgress.inc(0.2);[m
[32m+[m[32m      };[m
[32m+[m[32m      img.src = url;[m
[32m+[m[32m      if (img.complete) { img.onload(); }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var notLazyImages = jQuery('main img:not([lazyload])');[m
[32m+[m[32m    var total = notLazyImages.length;[m
[32m+[m[32m    for (const img of notLazyImages) {[m
[32m+[m[32m      const old = img.onload;[m
[32m+[m[32m      img.onload = function() {[m
[32m+[m[32m        old && old();[m
[32m+[m[32m        window.NProgress && window.NProgress.status !== null && window.NProgress.inc(0.5 / total);[m
[32m+[m[32m      };[m
[32m+[m[32m      if (img.complete) { img.onload(); }[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  registerRefreshCallback: function(callback) {[m
[32m+[m[32m    if (!Array.isArray(Fluid.events._refreshCallbacks)) {[m
[32m+[m[32m      Fluid.events._refreshCallbacks = [];[m
[32m+[m[32m    }[m
[32m+[m[32m    Fluid.events._refreshCallbacks.push(callback);[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  refresh: function() {[m
[32m+[m[32m    if (Array.isArray(Fluid.events._refreshCallbacks)) {[m
[32m+[m[32m      for (var callback of Fluid.events._refreshCallbacks) {[m
[32m+[m[32m        if (callback instanceof Function) {[m
[32m+[m[32m          callback();[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m
[32m+[m[32m  billboard: function() {[m
[32m+[m[32m    if (!('console' in window)) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m[32m    // eslint-disable-next-line no-console[m
[32m+[m[32m    console.log(`[m
[32m+[m[32m-------------------------------------------------[m
[32m+[m[32m|                                               |[m
[32m+[m[32m|      ________  __            _        __      |[m
[32m+[m[32m|     |_   __  |[  |          (_)      |  ]     |[m
[32m+[m[32m|       | |_ \\_| | | __   _   __   .--.| |      |[m
[32m+[m[32m|       |  _|    | |[  | | | [  |/ /'\`\\' |      |[m
[32m+[m[32m|      _| |_     | | | \\_/ |, | || \\__/  |      |[m
[32m+[m[32m|     |_____|   [___]'.__.'_/[___]'.__.;__]     |[m
[32m+[m[32m|                                               |[m
[32m+[m[32m|            Powered by Hexo x Fluid            |[m
[32m+[m[32m| https://github.com/fluid-dev/hexo-theme-fluid |[m
[32m+[m[32m|                                               |[m
[32m+[m[32m-------------------------------------------------[m
[32m+[m[32m    `);[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
