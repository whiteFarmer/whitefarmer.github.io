[32m+[m[32m      Fluid.utils.createScript('${url_join(theme.static_prefix.moment, 'locale/' + lang + '.min.js')}', function() {[m
[32m+[m[32m        relativeDate();[m
[32m+[m[32m      });[m
[32m+[m[32m    } else {[m
[32m+[m[32m      relativeDate();[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m</script>[m
[32m+[m[32m`)[m
