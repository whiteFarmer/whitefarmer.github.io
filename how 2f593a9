[1mdiff --git a/themes/fluid/scripts/helpers/utils.js b/themes/fluid/scripts/helpers/utils.js[m
[1mnew file mode 100644[m
[1mindex 0000000..7581d62[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/scripts/helpers/utils.js[m
[36m@@ -0,0 +1,29 @@[m
[32m+[m[32m/* global hexo */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst md5 = require('../utils/crypto');[m
[32m+[m[32mconst { decodeURL } = require('hexo-util');[m
[32m+[m[32mconst compareVersions = require('../../scripts/utils/compare-versions');[m
[32m+[m
[32m+[m[32mhexo.extend.helper.register('md5', function(string) {[m
[32m+[m[32m  return md5(string);[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mhexo.extend.helper.register('require_version', function(current, require) {[m
[32m+[m[32m  const verRe = current.match(/[@/](\d{1,2})\.?(\d{0,2})\.?(\d{0,2})/);[m
[32m+[m[32m  if (verRe && verRe.length >= 4) {[m
[32m+[m[32m    const ver = `${verRe[1]}.${verRe[2] || 'x'}.${verRe[3] || 'x'}`;[m
[32m+[m[32m    return compareVersions(ver, require) >= 0;[m
[32m+[m[32m  }[m
[32m+[m[32m  return false;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mhexo.extend.helper.register('deduplicate', function(arr) {[m
[32m+[m[32m  if (!Array.isArray(arr)) {[m
[32m+[m[32m    return [];[m
[32m+[m[32m  }[m
[32m+[m[32m  return [...new Set(arr)];[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mhexo.extend.helper.register('decode_url', decodeURL);[m
[1mdiff --git a/themes/fluid/scripts/helpers/wordcount.js b/themes/fluid/scripts/helpers/wordcount.js[m
[1mnew file mode 100644[m
[1mindex 0000000..cd6bd9f[m
[1m--- /dev/null[m
[1m+++ b/themes/fluid/scripts/helpers/wordcount.js[m
[36m@@ -0,0 +1,39 @@[m
[32m+[m[32m/* global hexo */[m
[32m+[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32mconst { stripHTML } = require('hexo-util');[m
[32m+[m
[32m+[m[32mconst getWordCount = (post) => {[m
[32m+[m[32m  if (!post.wordcount) {[m
[32m+[m[32m    // post.origin is the original post content of hexo-blog-encrypt[m
[32m+[m[32m    const content = stripHTML(post.origin || post.content).replace(/[\s\r\n]/g, '');[m
[32m+[m[32m    post.wordcount = content.length;[m
[32m+[m[32m  }[m
[32m+[m[32m  return post.wordcount;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mconst symbolsCount = (count) => {[m
[32m+[m[32m  if (count > 9999) {[m
[32m+[m[32m    count = Math.round(count / 1000) + 'k'; // > 9999 => 11k[m
[32m+[m[32m  } else if (count > 999) {[m
[32m+[m[32m    count = (Math.round(count / 100) / 10) + 'k'; // > 999 => 1.1k[m
[32m+[m[32m  } // < 999 => 111[m
[32m+[m[32m  return count;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mhexo.extend.helper.register('min2read', (post, { awl, wpm }) => {[m
[32m+[m[32m  return Math.floor(getWordCount(post) / ((awl || 2) * (wpm || 60))) + 1;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mhexo.extend.helper.register('wordcount', (post) => {[m
[32m+[m[32m  return symbolsCount(getWordCount(post));[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mhexo.extend.helper.register('wordtotal', (site) => {[m
[32m+[m[32m  let count = 0;[m
[32m+[m[32m  site.posts.forEach(post => {[m
[32m+[m[32m    count += getWordCount(post);[m
[32m+[m[32m  });[m
[32m+[m[32m  return symbolsCount(count);[m
[32m+[m[32m});[m
