[32m+[m[32m              <%- inject_point('postMarkdownBegin') %>[m
[32m+[m[32m              <%- page.content %>[m
[32m+[m[32m              <%- partial('_partials/plugins/encrypt') %>[m
[32m+[m[32m              <%- inject_point('postMarkdownEnd') %>[m
[32m+[m[32m            <% } else { %>[m
[32m+[m[32m              <div class="markdown-body">[m
[32m+[m[32m                <%- inject_point('postMarkdownBegin') %>[m
[32m+[m[32m                <%- page.content %>[m
[32m+[m[32m                <%- inject_point('postMarkdownEnd') %>[m
[32m+[m[32m              </div>[m
[32m+[m[32m            <% } %>[m
[32m+[m[32m            <hr/>[m
[32m+[m[32m            <div>[m
[32m+[m[32m              <%- inject_point('postMetaBottom') %>[m
[32m+[m
[32m+[m[32m              <%- inject_point('postCopyright') %>[m
[32m+[m
[32m+[m[32m              <% if (theme.post.prev_next.enable && !page.hide) { %>[m
[32m+[m[32m                <div class="post-prevnext my-3">[m
[32m+[m[32m                  <article class="post-prev col-6">[m
[32m+[m[32m                    <% const prev = prev_post(page) %>[m
[32m+[m[32m                    <% if (prev) { %>[m
[32m+[m[32m                      <a href="<%= url_for(prev.path) %>" title="<%= prev.title %>">[m
[32m+[m[32m                        <i class="iconfont icon-arrowleft"></i>[m
[32m+[m[32m                        <span class="hidden-mobile"><%= prev.title %></span>[m
[32m+[m[32m                        <span class="visible-mobile"><%- __('post.prev_post') %></span>[m
[32m+[m[32m                      </a>[m
[32m+[m[32m                    <% } %>[m
[32m+[m[32m                  </article>[m
[32m+[m[32m                  <article class="post-next col-6">[m
